<script type="text/javascript">
P.modules.loadModule("poll_results", {}, null, "\n\u003cdiv class=\"post_region_box\" id=\"poll_results\"\u003e\n{{#unless toShow.poll_unpublished}}\n  \u003cdiv class=\"post_region_content member_answer\"\u003e\n\n{{#if toShow.is_instructor}}\n\n    {{#if toShow.poll_closed}}\n    \u003ch1 id=\"poll_closed\" class=\"show\"\u003e\u003cb\u003e{{content.subject}}\u003c/b\u003e is now closed\u003c/h1\u003e\n    {{/if}}\n\n    {{#if toShow.poll_will_close}}\n    \u003ch1 id=\"poll_will_close\" class=\"show\"\u003e\u003cb\u003e{{content.subject}}\u003c/b\u003e closes in \u003cspan class=\"poll_closes_in\"\u003e{{toShow.poll_closes_in}}\u003c/span\u003e\u003c/h1\u003e\n    {{/if}}\n\n{{else}}\n\n    {{#if toShow.poll_submitted}}\n    \u003ch1 id=\"poll_submitted\" class=\"show\"\u003eThanks for submitting your response!\u003c/h1\u003e\n    {{/if}}\n    {{#if toShow.poll_not_submitted}}\n    \u003ch1 id=\"poll_not_submitted\" class=\"show\"\u003eSubmit your vote to see results\u003c/h1\u003e\n    {{/if}}\n\n    {{#if toShow.poll_closed}}\n    \u003ch1 id=\"poll_closed\" class=\"show\"\u003e\u003cb\u003e{{content.subject}}\u003c/b\u003e is now closed\u003c/h1\u003e\n    {{/if}}\n    {{#if toShow.poll_not_closed}}\n    \u003ch1 id=\"poll_not_closed\" class=\"show\"\u003eResults are visible after the poll closes in \u003cspan class=\"poll_closes_in\"\u003e{{toShow.poll_closes_in}}\u003c/span\u003e\u003c/h1\u003e\n    {{/if}}\n    {{#if toShow.poll_will_close}}\n    \u003ch1 id=\"poll_will_close\" class=\"show\"\u003e\u003cb\u003e{{content.subject}}\u003c/b\u003e closes in \u003cspan class=\"poll_closes_in\"\u003e{{toShow.poll_closes_in}}\u003c/span\u003e\u003c/h1\u003e\n    {{/if}}\n\n    {{#if toShow.poll_only_instructors}}\n    \u003ch1 id=\"poll_only_instructors\" class=\"show\"\u003eResult is only visible to instructors\u003c/h1\u003e\n    {{/if}}\n\n    {{#if toShow.poll_no_data}}\n    \u003ch1 id=\"poll_no_data\" class=\"show\"\u003eThere are no results yet\u003c/h1\u003e\n    {{/if}}\n\n{{/if}}\n\n\n\n{{#if toShow.is_instructor}}\n    {{#ifGreater content.data.total_votes 0}}\n\n    \u003ch4\u003eA total of \u003cb\u003e\u003cspan class=\"num_poll_votes\"\u003e{{content.data.total_votes}}\u003c/span\u003e\u003c/b\u003e vote(s) in \u003cb\u003e\u003cspan class=\"poll_time\"\u003e{{toShow.poll_time}}\u003c/span\u003e\u003c/b\u003e hours\u003c/h4\u003e\n    {{else}}\n        \u003ch4\u003eA total of 0 votes in \u003cb\u003e\u003cspan class=\"poll_time\"\u003e{{toShow.poll_time}}\u003c/span\u003e\u003c/b\u003e hours\u003c/h4\u003e\n    {{/ifGreater}}\n    {{#if toShow.poll_toggle_visibility_on}}\n    \u003cdiv class=\"poll_results_wrapper\"\u003e\n      {{#each results}}\n        {{#pollResult this}}{{/pollResult}}\n      {{/each}}\n    \u003c/div\u003e\n\n    {{else}}\n    \u003ch1 class=\"show\"\u003ePoll results are hidden.  To view, click Show Poll Results in the admin panel.\u003c/h1\u003e\n\n\n    {{/if}}\n\n{{else}}\n    {{#if toShow.poll_show_results}}\n    {{#ifGreater content.data.total_votes 0}}\n    \u003ch4\u003eA total of \u003cb\u003e\u003cspan class=\"num_poll_votes\"\u003e{{content.data.total_votes}}\u003c/span\u003e\u003c/b\u003e vote(s) in \u003cb\u003e\u003cspan class=\"poll_time\"\u003e{{toShow.poll_time}}\u003c/span\u003e\u003c/b\u003e hours\u003c/h4\u003e\n    {{else}}\n            \u003ch4\u003eA total of 0 votes in \u003cb\u003e\u003cspan class=\"poll_time\"\u003e{{toShow.poll_time}}\u003c/span\u003e\u003c/b\u003e hours\u003c/h4\u003e\n\n    {{/ifGreater}}\n    {{#if toShow.poll_toggle_visibility_on}}\n    \u003cdiv class=\"poll_results_wrapper\"\u003e\n      {{#each results}}\n        {{#pollResult this}}{{/pollResult}}\n      {{/each}}\n    \u003c/div\u003e\n    {{else}}\n    \u003ch1 class=\"show\"\u003ePoll results are hidden by your instructor.\u003c/h1\u003e\n    {{/if}}\n    {{/if}}\n\n{{/if}}\n\n    {{#if toShow.poll_closed}}\n        {{#if content.data.answer}}\n            {{#ifNotEquals content.data.answer \"\"}}\n                {{#ifNotEquals content.data.answer \"[ ]\"}}\n                    \u003ch1 id=\"poll_closed_answer\" class=\"show\"\u003eExplanation: \u003cb\u003e{{content.data.answer}}\u003c/b\u003e\u003c/h1\u003e\n                {{/ifNotEquals}}\n            {{/ifNotEquals}}\n        {{/if}}\n    {{/if}}\n  \u003c/div\u003e\n\n\n{{/unless}}\n\n\u003c/div\u003e\n\n");</script>

<script type="text/javascript">
P.modules.loadModule("answer", {}, null, "\u003cdiv class=\"post_region_box answer\" id=\"{{answer.type}}\"\u003e\n  \u003cdiv class=\"post_region_header clearFix\"\u003e\n    \u003cdiv class=\"post_icon {{answer.icon}}\"\u003e\u003c/div\u003e\n    \u003cdiv class=\"post_title\"\u003e{{answer.title}},\u003c/div\u003e\n    \u003cdiv class=\"post_subtitle\"\u003e{{answer.long}}\u003c/div\u003e\n  \u003c/div\u003e\n  {{#if answer.showEdit}}\n  \u003cdiv class=\"dropdown post_region_arrow_dropdown\"\u003e\n    \u003ca href=\"#\" class=\"dropdown-toggle\" data-toggle=\"dropdown\"\u003eActions \u003cb class=\"caret\"\u003e\u003c/b\u003e\u003c/a\u003e\n    \u003cdiv class=\"dropdown_menu_wrapper clearFix\"\u003e\n      \u003cul class=\"dropdown-menu\" role=\"menu\" id=\"answer_actions_dropdown\" aria-labelledby=\"dLabel\"\u003e\n        \u003c!--\u003cli class=\"attach\"\u003e\u003ca class=\"attach\" href=\"#\"\u003eAttach File\u003c/a\u003e\u003c/li\u003e--\u003e\n        \u003cli class=\"report_spam\"\u003e\u003ca class=\"report_spam\" href=\"#\"\u003eFlag as Inappropriate\u003c/a\u003e\u003c/li\u003e\n      \u003c/ul\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  {{/if}}\n  {{#if answer.updated}}\n    \u003cdiv class=\"post_region_content view_mode\"\u003e\n    {{#answerText answer network}}{{/answerText}}\n    \u003c/div\u003e\n  {{/if}}\n  {{#if answer.isActiveTimer}}\n    {{#unless answer.updated}}\n    \u003cdiv class=\"qa_timer_container\"\u003e\n      \u003cdiv class=\"qa_timer_option_container\"\u003e\n        Instructors are encouraging students to attempt to answer this question.\n      \u003c/div\u003e\n    \u003c/div\u003e\n    {{/unless}}\n  {{/if}}\n  {{#if answer.isInstructor}}\n    {{#if network.config.timer.enabled}}\n      {{#unless answer.updated}}\n  \u003cdiv class=\"qa_timer_container\"\u003e\n    \u003cdiv class=\"qa_timer_option_container\"\u003e\n      {{#unless content.config.timer.enabled}}\n      \u003cdiv class=\"timer_delay_icon small grey\"\u003e\u003c/div\u003e\n      \u003ca href=\"#\" onclick=\"return P.answer.setTimer(P.answer.network.config.timer.delay)\" class=\"set_timer_link\"\u003eSet timer\u003c/a\u003e\n      {{/unless}}\n      {{#if content.config.timer.delay}}\n        {{#if content.config.timer.isActive}}\n          \u003cdiv class=\"timer_delay_icon small green\"\u003e\u003c/div\u003e\n          \u003cspan class=\"timer_set_text\"\u003eTimer set\u003c/span\u003e\n          {{#if content.config.timer.user_id}}\n            \u003cspan class=\"timer_set_meta\"\u003e{{#toRelativeTime content.config.timer.when}}{{/toRelativeTime}} by {{#toUserName content.config.timer.user_id}}{{/toUserName}}\u003c/span\u003e\n          {{else}}\n            \u003cspan class=\"timer_set_meta\"\u003eDefault timer set {{#toRelativeTime content.config.timer.when}}{{/toRelativeTime}}\u003c/span\u003e\n          {{/if}}\n        {{else}}\n          \u003cdiv class=\"timer_delay_icon small red\"\u003e\u003c/div\u003e\n          \u003cspan class=\"timer_set_text\"\u003eTimer done\u003c/span\u003e\n          {{#if content.config.timer.user_id}}\n            \u003cspan class=\"timer_set_meta\"\u003e{{#toRelativeTime content.config.timer.when}}{{/toRelativeTime}} by {{#toUserName content.config.timer.user_id}}{{/toUserName}}\u003c/span\u003e\n          {{else}}\n            \u003cspan class=\"timer_set_meta\"\u003eDefault timer set {{#toRelativeTime content.config.timer.when}}{{/toRelativeTime}}\u003c/span\u003e\n          {{/if}}\n        {{/if}}\n      {{/if}}\n    \u003c/div\u003e\n    {{#if content.config.timer.enabled}}\n    \u003cdiv class=\"qa_timer_length_container\"\u003e\n      \u003cspan\u003eTimer length:\u003c/span\u003e\n      \u003clabel for=\"thirty_min\"\u003e\n        \u003cinput type=\"radio\" id=\"thirty_min\" onchange=\"P.answer.setTimer(1)\" name=\"timer_length\" {{#ifEquals content.config.timer.delay 1}}checked{{/ifEquals}}/\u003e\n        \u003cspan\u003e30 minutes\u003c/span\u003e\n      \u003c/label\u003e\n      \u003clabel for=\"two_hours\"\u003e\n        \u003cinput type=\"radio\" id=\"timer_2\" onchange=\"P.answer.setTimer(4)\" name=\"timer_length\" {{#ifEquals content.config.timer.delay 4}}checked{{/ifEquals}}/\u003e\n        \u003cspan\u003e2 hours\u003c/span\u003e\n      \u003c/label\u003e\n      \u003clabel for=\"four_hours\"\u003e\n        \u003cinput type=\"radio\" id=\"timer_4\" onchange=\"P.answer.setTimer(8)\" name=\"timer_length\" {{#ifEquals content.config.timer.delay 8}}checked{{/ifEquals}}\u003e\n        \u003cspan\u003e4 hours\u003c/span\u003e\n      \u003c/label\u003e\n      \u003clabel for=\"eight_hours\"\u003e\n        \u003cinput type=\"radio\" id=\"timer_8\" onchange=\"P.answer.setTimer(16)\" name=\"timer_length\" {{#ifEquals content.config.timer.delay 16}}checked{{/ifEquals}}\u003e\n        \u003cspan\u003e8 hours\u003c/span\u003e\n      \u003c/label\u003e\n      \u003clabel for=\"twenty_four_hours\"\u003e\n        \u003cinput type=\"radio\" id=\"timer_24\" onchange=\"P.answer.setTimer(48)\" name=\"timer_length\" {{#ifEquals content.config.timer.delay 48}}checked{{/ifEquals}}\u003e\n        \u003cspan\u003e24 hours\u003c/span\u003e\n      \u003c/label\u003e\n      \u003clabel for=\"timer_off\"\u003e\n        \u003cinput type=\"radio\" id=\"timer_off\" onchange=\"P.answer.setTimer(false)\" name=\"timer_length\" {{#ifEquals content.config.timer.delay false}}checked{{/ifEquals}}\u003e\n        \u003cspan\u003eoff\u003c/span\u003e\n      \u003c/label\u003e\n    \u003c/div\u003e\n    {{/if}}\n  \u003c/div\u003e\n      {{/unless}}\n    {{/if}}\n  {{/if}}\n  \u003cdiv class=\"post_region_content edit_mode\" id=\"edit_member_answer\" style=\"display:none;\"\u003e\n    {{#if answer.showSuper}}\n    \u003cdiv class=\"message-alert super-edit-answer\" id=\"super-answer-2\" style=\"background-color: rgb(248, 202, 202); margin-bottom: 7px; color: rgb(153, 0, 0); border: 1px solid rgb(153, 0, 0); display: block; background-position: initial initial; background-repeat: initial initial;\"\u003e\n      \u003cspan class=\"message\"\u003e\n        \u003cem\u003e!\u003c/em\u003e\u0026nbsp;\u0026nbsp;As an instructor, you're editing the Students' Response in super-user mode.\n      \u003c/span\u003e\n    \u003c/div\u003e\n    {{/if}}\n    {{#ifEquals user.config.default_editor \"plain\"}}\n    \u003cspan style=\"float: right\" class=\"input_links grey_text\"\u003e\u003ca href=\"#\" class=\"richtext_toggle\" onclick=\"return PEM.fire('toggleRichtext')\"\u003euse rich text editor\u003c/a\u003e\u003c/span\u003e\n    {{else}}\n    \u003cspan style=\"float: right\" class=\"input_links grey_text\"\u003e\u003ca href=\"#\" class=\"richtext_toggle\" onclick=\"return PEM.fire('toggleRichtext')\"\u003euse plain text editor\u003c/a\u003e\u003c/span\u003e\n    {{/ifEquals}}\n    \u003cspan class=\"input_links grey_text\" style=\"margin-right:10px;\"\u003eReport any bugs with our editor to {{#if network.isGroup}}groups-bugs@piazza.com{{else}}bugs@piazza.com{{/if}}\u003c/span\u003e\n    \u003ch3\u003eEdit Answer\u003c/h3\u003e\n    \u003cdiv class=\"answer_edit_wrapper\"\u003e\u003c/div\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"post_region_message_wrapper currently_editing\" style=\"display:none;margin-top:3px;\"\u003e\n    \u003cdiv class=\"post_region_message warning show\"\u003e\n      \u003cspan class=\"editing_message\"\u003eThis is red text\u003c/span\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"post_region_actions edit_mode\"\u003e\n    \u003ca data-pats=\"submit_button\" class=\"post_action submit main btn btn-primary btn-mini\" href=\"#\"\u003esubmit\u003c/a\u003e\n    {{#if anon}}\n     \u0026nbsp;\u0026nbsp;as \u003cselect class=\"anonymity_selector\"\u003e\n        {{#each anon}}\n        \u003coption value=\"{{this.val}}\"\u003e{{this.title}}\u003c/option\u003e\n        {{/each}}\n      \u003c/select\u003e\n    {{/if}}\n    \u003cspan class=\"middot\"\u003e\u0026middot;\u003c/span\u003e\n    \u003ca class=\"post_action preview\" href=\"#\" onclick=\"return PEM.fire('previewPost', 'answer_{{answer.type}}')\"\u003epreview\u003c/a\u003e\n    \u003ca class=\"post_action cancel right\" href=\"#\"\u003ecancel\u003c/a\u003e\n  \u003c/div\u003e\n  {{#if answer.updated}}\n    \u003cdiv class=\"post_region_actions view_mode\"\u003e\n      {{#if answer.showEdit}}\n      \u003ca data-pats=\"edit_button\" class=\"post_action edit main btn btn-mini btn-primary\" tutorial=\"Edit the single Wiki Answer to improve the answer.\" href=\"#\"\u003eedit\u003c/a\u003e\n      \u003cspan class=\"middot\"\u003e\u0026middot;\u003c/span\u003e\n      {{/if}}\n      {{#if endorsed}}\n        \u003ca class=\"post_action undo_good_answer\" href=\"#\"\u003eundo {{#if user.isInst}}good answer{{else}}thanks{{/if}}\u003c/a\u003e\n        \u003ca class=\"post_action do_good_answer\" style=\"display:none;\" href=\"#\"\u003e{{#if user.isInst}}good answer{{else}}thanks!{{/if}}\u003c/a\u003e\n      {{else}}\n        \u003ca class=\"post_action undo_good_answer\" style=\"display:none;\" href=\"#\"\u003eundo {{#if user.isInst}}good answer{{else}}thanks{{/if}}\u003c/a\u003e\n        \u003ca class=\"post_action do_good_answer\" href=\"#\"\u003e{{#if user.isInst}}good answer{{else}}thanks!{{/if}}\u003c/a\u003e\n      {{/if}}\n      \u003cspan class=\"post_actions_number good_answer\"\u003e{{answer.endorseCnt}}\u003c/span\u003e\n      \u003cdiv class=\"post_region_actions_meta\"\u003eUpdated {{#toRelativeTime answer.updated}}{{/toRelativeTime}} by\n        {{#if answer.uid}}\n          {{#toUserName answer.uid answer.anon}}{{/toUserName}}\n        {{else}}\n          \u003cspan class=\"privatePoster highlight\"\u003eAnonymous\u003c/span\u003e\n        {{/if}}\n        {{#showOthers answer.others}}{{/showOthers}}\n      \u003c/div\u003e\n    \u003c/div\u003e\n  {{else}}\n    \u003cdiv class=\"post_region_content compose_mode\"\u003e\n      \u003cdiv data-pats=\"start_textbox\" class=\"compose_answer\"\u003eClick to start off the wiki answer\u003c/div\u003e\n    \u003c/div\u003e\n  {{/if}}\n\u003c/div\u003e\n");</script>

