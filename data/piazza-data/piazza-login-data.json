<script type="text/javascript">
P.modules.loadModule("stats", {}, null, "\u003cdiv id=\"ClassStatisticsPanel\" class=\"mainPanel post_region_box\"\u003e\n  \u003cdiv class=\"scrollable-wrapper\"\u003e\n    \u003cdiv class=\"scrollable\"\u003e\n      \u003cdiv class=\"subregion clearfix\"\u003e\n        \u003cdiv class=\"subregionContent\"\u003e\n          \u003cdiv id=\"reportHeader\"\u003e\n            \u003ch1\u003e\u003cspan class=\"course-number\"\u003e{{network.course_number}}\u003c/span\u003e Statistics \u003cspan style=\"font-weight: normal;font-size:13px;\" id=\"classStatisticLink\"\u003e\u003c/span\u003e\u003c/h1\u003e\n            \u003cspan style=\"font-weight: normal;font-size:13px;\" id=\"pollStatisticLink\"\u003e\u003c/span\u003e\n          \u003c/div\u003e\n          \u003cdiv id=\"chartsRegion\" class=\"infoBox\"\u003e\n            \u003ch2\u003eUsage Trends for \u003cspan class=\"course-number\"\u003e{{network.course_number}}\u003c/span\u003e\u003c/h2\u003e\n            \u003cdiv class=\"content align-center\"\u003e\n              \u003cdiv class=\"chart\" id=\"googleChart\"\u003e\u003c/div\u003e\n              \u003cdiv class=\"chartTitle\" id=\"usersPerHourChartTitle\"\u003e\n                \u003cselect id=\"stats_chart_selector\" onchange=\"P.stats.showStatsChart();return false;\"\u003e\n                  \u003coption value=\"users\" selected\u003eUnique users per day\u003c/option\u003e\n                  \u003coption value=\"questions\"\u003ePosts per day\u003c/option\u003e\n                \u003c/select\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"sidebar\"\u003e\n            \u003cdiv id=\"weekOverview\" class=\"infoBox\"\u003e\n              \u003ch2\u003eClass at a Glance\u003c/h2\u003e\n              \u003cdiv class=\"content\"\u003e\n                \u003cul\u003e\n                  \u003cli class=\"clearFix\"\u003e\n                    \u003cdiv class=\"value\" id=\"stats_questions\"\u003e\u003c/div\u003e\n                    \u003cdiv class=\"description\"\u003etotal posts\u003cem\u003e*\u003c/em\u003e\u003c/div\u003e\n                  \u003c/li\u003e\n                  \u003cli class=\"clearFix\"\u003e\n                    \u003cdiv class=\"value\" id=\"stats_posts\"\u003e\u003c/div\u003e\n                    \u003cdiv class=\"description\"\u003etotal contributions\u003cem\u003e**\u003c/em\u003e\u003c/div\u003e\n                  \u003c/li\u003e\n                  \u003cli class=\"clearFix\"\u003e\n                    \u003cdiv class=\"value\" id=\"stats_ir\"\u003e\u003c/div\u003e\n                    \u003cdiv class=\"description\"\u003einstructors' responses\u003c/div\u003e\n                  \u003c/li\u003e\n                  \u003cli class=\"clearFix\"\u003e\n                    \u003cdiv class=\"value\" id=\"stats_sr\"\u003e\u003c/div\u003e\n                    \u003cdiv class=\"description\"\u003estudents' responses\u003c/div\u003e\n                  \u003c/li\u003e\n                  \u003cli class=\"clearFix\"\u003e\n                    \u003cdiv class=\"value\" id=\"stats_rt\"\u003e\u003c/div\u003e\n                    \u003cdiv class=\"description\"\u003eavg. response time\u003c/div\u003e\n                  \u003c/li\u003e\n                \u003c/ul\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n          \u003cdiv id=\"topParticipantsRegion\"\u003e\n            \u003cdiv class=\"infoBoxWrapper\"\u003e\n              \u003cdiv id=\"topAskers\" class=\"infoBox\"\u003e\n                \u003ch2\u003eTop Student Contributors\u003c/h2\u003e\n                \u003cdiv class=\"content\"\u003e\n                  \u003ctable id=\"stats_most_active\" class=\"table table-condensed table-hover table-bordered\"\u003e\u003c/table\u003e\n                \u003c/div\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n            \u003cdiv class=\"clearFix\"\u003e\u0026nbsp;\u003c/div\u003e\n            \u003cdiv style=\"width: 98%;\" id=\"top_askers\"\u003e\n              \u003cdiv class=\"infoBox\"\u003e\n                \u003ch2\u003eTop Student Askers\u003c/h2\u003e\n                \u003cdiv class=\"content\"\u003e\n                  \u003ctable id=\"stats_askers\" class=\"tablesorter table table-condensed table-hover table-bordered\"\u003e\u003c/table\u003e\n                \u003c/div\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n            \u003cdiv style=\"width: 98%;\" id=\"top_good_q\"\u003e\n              \u003cdiv class=\"infoBox\"\u003e\n                \u003ch2\u003eTop Student \u0026quot;Good Question\u0026quot; Askers\u003c/h2\u003e\n                \u003cdiv class=\"content\"\u003e\n                  \u003ctable id=\"stats_good_q\" class=\"tablesorter table table-condensed table-hover table-bordered\"\u003e\u003c/table\u003e\n                \u003c/div\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n            \u003cdiv style=\"width: 98%;\" id=\"top_answerers\"\u003e\n              \u003cdiv class=\"infoBox\"\u003e\n                \u003ch2\u003eTop Student Answerers\u003c/h2\u003e\n                \u003cdiv class=\"content\"\u003e\n                  \u003ctable id=\"stats_answerers\" class=\"tablesorter table table-condensed table-hover table-bordered\"\u003e\u003c/table\u003e\n                \u003c/div\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n            \u003cdiv style=\"width: 98%;\" id=\"top_good_a\"\u003e\n              \u003cdiv class=\"infoBox\"\u003e\n                \u003ch2\u003eTop Student \u0026quot;Endorsed Answer\u0026quot; Answerers\u003c/h2\u003e\n                \u003cdiv class=\"content\"\u003e\n                  \u003ctable id=\"stats_good_a\" class=\"tablesorter table table-condensed table-hover table-bordered\"\u003e\u003c/table\u003e\n                \u003c/div\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n            \u003cdiv style=\"width: 98%;\" id=\"top_listeners\"\u003e\n              \u003cdiv class=\"infoBox\"\u003e\n                \u003ch2\u003eTop Student Listeners\u003c/h2\u003e\n                \u003cdiv class=\"content\"\u003e\n                  \u003ctable id=\"stats_listeners\" class=\"tablesorter table table-condensed table-hover table-bordered\"\u003e\u003c/table\u003e\n                \u003c/div\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n            \u003cdiv id=\"stats_notes\"\u003e\n              \u003cp\u003e\n                \u003cem\u003e\u003cstrong\u003e*posts\u003c/strong\u003e\u003c/em\u003e\u0026nbsp;are\u0026nbsp;questions\u0026nbsp;and\u0026nbsp;notes\u003cbr /\u003e\n                \u003cem\u003e\u003cstrong\u003e**contributions\u003c/strong\u003e\u003c/em\u003e are posts, responses, edits, followups, and comments to followups (i.e., everything)\n              \u003c/p\u003e\n            \u003c/div\u003e\n            \u003cdiv class=\"infoBoxWrapper\" style=\"width: 98%;\" id=\"prof_participation\"\u003e\n              \u003cdiv class=\"infoBox\"\u003e\n                \u003ch2\u003eInstructor Participation Report\u003c/h2\u003e\n                \u003cdiv class=\"content\"\u003e\n                  \u003ctable id=\"stats_all_instructors\" class=\"tablesorter table table-condensed table-hover table-bordered\"\u003e\u003c/table\u003e\n                \u003c/div\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n            \u003cdiv class=\"infoBoxWrapper\" style=\"width: 98%;\" id=\"stud_participation\"\u003e\n              \u003cdiv class=\"infoBox\"\u003e\n                \u003ch2 id=\"student_participation_name\"\u003eStudent Participation Report\u003c/h2\u003e\n                \u003cdiv class=\"content\"\u003e\n                  \u003ctable id=\"stats_all_users\" class=\"tablesorter table table-condensed table-hover table-bordered\"\u003e\u003c/table\u003e\n                \u003c/div\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n            \u003cdiv class=\"infoBoxWrapper\" style=\"width: 98%;\" id=\"more_info_coming\"\u003e\n              \u003cdiv class=\"infoBox\"\u003e\n                \u003cdiv class=\"content\"\u003e\n                  More stats will be displayed here as class activity increases.\n                \u003c/div\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n");</script>

<script type="text/javascript">
P.modules.loadModule("upload_manager", {}, null, "\u003cdiv id=\"upload_manager\" class=\"modal hide fade\"\u003e\n  \u003cdiv class=\"modal-header\"\u003e\n    \u003cbutton type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-hidden=\"true\"\u003e\u0026times;\u003c/button\u003e\n    \u003ch3\u003eMy File Manager\u003c/h3\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"modal-body\"\u003e\n    \u003cbutton class=\"btn btn-primary browse_upload\" id=\"browse_upload\"\u003eUpload a new file\u003c/button\u003e or select previously uploaded file from below.\n    \u003cdiv class=\"manage_files_section\"\u003e\n      \u003cdiv class=\"browse_upload_files tab-content\" id=\"browse_upload_files_{{data.nid}}\"\u003e\n        {{#each data.files}}\n        \u003cdiv class=\"browse_upload_one_file\" id=\"file_{{this.fid}}\"\u003e\n          \u003ca href=\"#\" onclick=\"return P.upload_manager.submit('{{this.url}}', '{{this.name}}');\"\u003e\n            \u003cimg src=\"{{#extensionIcon this.name}}{{/extensionIcon}}\"/\u003e\n            {{this.name}}\n          \u003c/a\u003e, \n          \u003cspan class=\"size\"\u003e{{this.size}}\u003c/span\u003e\n          \u003cdiv class=\"remove_button\"\u003e\n            \u003ca href=\"#\" onclick=\"return P.upload_manager.deleteFile('{{this.fid}}');\"\u003e(remove)\u003c/a\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        {{/each}}\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"modal-footer\"\u003e\n    \u003ca href=\"#\" class=\"btn cancel\" onclick=\"return P.upload_manager.cancel();\"\u003eCancel\u003c/a\u003e\n  \u003c/div\u003e\n\u003c/div\u003e");</script>

<script type="text/javascript">
P.modules.loadModule("drafts", {}, null, "\u003cdiv class=\"question_group\" id=\"draft_group\"\u003e\n  \u003cdiv title=\"Drafts\" onclick=\"P.drafts.toggleFolder()\" id=\"bucket_draft\" class=\"start_open drafts\"\u003e\u003cspan\u003e▾ Drafts\u003c/span\u003e\u003c!--\u003cimg src=\"/images/piazza/dashboard/feed_icon_draft.png\" alt=\"drafts\"\u003e--\u003e\u003c/div\u003e\n  \u003cul id=\"bucket_draft-item-list\" class=\"item-list-in-bucket\"\u003e\n    {{#each drafts}}\n      \u003cli class=\"feed_item clearfix\" id=\"{{this.id}}\" onclick=\"P.drafts.loadDraft('{{this.id}}')\"\u003e\n        \u003cdiv id=\"FeedItemDropdown_{{content.id}}\" class=\"feed_item_dropdown_selector closed memoryless clearFix\"\u003e\n          \u003cdiv class=\"feed_item_dropdown_arrow\"\u003e\u0026#9660;\u003c/div\u003e\n          \u003cul\u003e\n            \u003cli class=\"delete_draft_action\" onclick=\"return P.drafts.deleteDraft('{{this.id}}');\"\u003eDelete \u003cem\u003e(from your list of drafts)\u003c/em\u003e\u003c/li\u003e\n          \u003c/ul\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"content\"\u003e\n          \u003cdiv class=\"title ellipses\"\u003e\n            \u003cspan class=\"title_text\"\u003e{{this.txt.post_summary}}\u003c/span\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"snippet\"\u003e{{this.content}}\u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/li\u003e\n    {{/each}}\n  \u003c/ul\u003e\n\u003c/div\u003e");</script>

<script type="text/javascript">
P.modules.loadModule("old_new_post", {}, null, "\u003cdiv class=\"old_new_post clearFix dashboard_element\" id=\"old_new_post_window\"\u003e\n  \u003cdiv class=\"post_message\" id=\"no_tags_error\"\u003e\n    \u003ch6\u003eBefore submitting:\u003c/h6\u003e\n    \u003cul\u003e\n      \u003cli class=\"no_title_error\"\u003e\n        Give your \u003cspan class=\"show_for_question\"\u003equestion\u003c/span\u003e\u003cspan class=\"show_for_note\"\u003enote\u003c/span\u003e\u003cspan class=\"show_for_poll\"\u003epoll\u003c/span\u003e \u003cem\u003ea brief, descriptive title\u003c/em\u003e.\n      \u003c/li\u003e\n      \u003cli class=\"no_tags_error\"\u003e\n        Your post has no tags! Tags make it easy to filter and find content. Click on a tag below or type \u0026quot;#\u0026quot; to start off a new tag. (You can also choose to continue without tagging.)\n      \u003c/li\u003e\n    \u003c/ul\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"post_message\" id=\"new_post_alert_announcement\"\u003e\n    \u003ch6\u003eThis note will be an Announcement\u003c/h6\u003e\n    \u003cul\u003e\n      \u003cli\u003e\n        The checkbox in the \"Posting Options\" below indicates that this note will be an announcement.\n      \u003c/li\u003e\n    \u003c/ul\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"posting_meta_section post_type clearFix\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003ePost Type\u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      \u003cdiv class=\"post_type_box active\"\u003e\n        \u003clabel class=\"radio\"\u003e\n          \u003cinput type=\"radio\" name=\"post_type\" id=\"post_type_question\" value=\"option1\" checked/\u003e\n          Question\n          \u003cem\u003eif you need an answer\u003c/em\u003e\n        \u003c/label\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"post_type_box\"\u003e\n        \u003clabel class=\"radio\"\u003e\n          \u003cinput type=\"radio\" name=\"post_type\" id=\"post_type_note\" value=\"option2\"/\u003e\n          Note\n          \u003cem\u003eif you \u003cb\u003edon't\u003c/b\u003e need an answer\u003c/em\u003e\n        \u003c/label\u003e\n      \u003c/div\u003e\n     {{#if isInst}}      \n      \u003cdiv class=\"post_type_box\"\u003e\n        \u003clabel class=\"radio\"\u003e\n          \u003cinput type=\"radio\" name=\"post_type\" id=\"post_type_poll\" value=\"option3\"/\u003e\n          Poll/In-Class Response\n          \u003cem\u003eif you need a vote\u003c/em\u003e\n        \u003c/label\u003e\n      \u003c/div\u003e\n     {{else}}\n\t     {{#unless network.config.disable_student_polls}}\n\t\t      \u003cdiv class=\"post_type_box\"\u003e\n\t\t        \u003clabel class=\"radio\"\u003e\n\t\t          \u003cinput type=\"radio\" name=\"post_type\" id=\"post_type_poll\" value=\"option3\"/\u003e\n\t\t          Poll/In-Class Response\n\t\t          \u003cem\u003eif you need a vote\u003c/em\u003e\n\t\t        \u003c/label\u003e\n\t\t      \u003c/div\u003e\n\t     {{/unless}}\n     {{/if}}\n    \u003c/div\u003e\n  \u003c/div\u003e\n  {{#if feedback}}\n  \u003cdiv class=\"duplicate_question_feedback_section clearFix show_for_note_question\"\u003e\n    \u003cdiv class=\"duplicate_question_feedback_container clearfix {{feedback_class}}\"\u003e\n      \u003cdiv class=\"left_section main_label\"\u003eDuplicate question feedback\u003c/div\u003e\n      \u003cdiv class=\"right_section\"\u003e\n        {{#ifEquals feedback_class \"medium\"}}\n        \u003cp class=\"feedback_message\"\u003eYou are still receiving duplicate question feedback on your questions.\u003c/p\u003e\n        {{else}}\n        \u003cp class=\"feedback_message\"\u003eYou've received duplicate question feedback on questions you've asked.\u003c/p\u003e\n        {{/ifEquals}}\n        \u003cul class=\"question_feedback_list\"\u003e\n          {{#each feedback}}\n          \u003cli\u003e\u003cspan\u003e{{this.name}} ({{this.cnt}} times)\u003c/span\u003e\u003c/li\u003e\n          {{/each}}\n        \u003c/ul\u003e\n        {{#ifEquals feedback_class \"medium\"}}\n        \u003cp class=\"feedback_question_suggestion\"\u003e\u003cstrong\u003eWarning:\u003c/strong\u003e if you continue to receive more feedback, your ability to post questions may be temporarily suspended.\u003c/p\u003e\n        {{else}}\n        \u003cp class=\"feedback_question_suggestion\"\u003e\u003cstrong\u003eSuggestion:\u003c/strong\u003e please review questions on Piazza and information provided in course materials ahead of posting new questions.\u003c/p\u003e\n        {{/ifEquals}}\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  {{/if}}\n  \u003cdiv class=\"posting_meta_section post_to clearFix show_for_note_question\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003ePost to\u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      \u003clabel class=\"radio\"\u003e\n        \u003cinput type=\"radio\" name=\"post_to\" id=\"entire_group\" value=\"option1\" checked\u003e\n        {{#if network.isGroup}}\n        Entire Group\n        {{else}}\n        Entire Class\n        {{/if}}\n      \u003c/label\u003e\n      {{#if network.subGroups}}\n      \u003clabel class=\"radio\"\u003e\n        \u003cinput type=\"radio\" name=\"post_to\" id=\"class_subgroup\" value=\"option2\"\u003e\n        Class Group\n      \u003c/label\u003e\n      {{/if}}\n      {{#unless network.noPrivatePosts}}\n      \u003clabel class=\"radio\"\u003e\n        \u003cinput type=\"radio\" name=\"post_to\" id=\"individual_members\" value=\"option3\"\u003e\n        {{#if network.isGroup}}\n        Individual Member(s)\n        {{else}}\n          {{#if isInst}}\n            Individual Student(s) / Instructor(s)\n          {{else}}\n            Instructor(s)\n          {{/if}}\n        {{/if}}\n      \u003c/label\u003e\n\n       {{/unless}}\n      \u003cdiv class=\"new_post_error too_many_names\"\u003eYou can send individual posts to up to 10 people\u003c/div\u003e\n      \u003cinput id=\"member_names\" class=\"new_post_textarea\" type=\"text\" placeholder=\"Enter one or more names...\" style=\"display: none;\"\u003e\n      \u003cdiv id=\"member_names_message\" class=\"small_grey_text\" style=\"display: none\"\u003eType \"Instructors\" to include all instructors.\u003c/div\u003e\n      \u003cdiv class=\"new_post_error no_names\"\u003eEnter at least one name before submitting this post\u003c/div\u003e\n      \u003cul class=\"dropdown-menu user_list general_dropdown\" id=\"member_names_dropdown_list\" role=\"menu\" aria-labelledby=\"dropdownMenu\"\u003e\u003c/ul\u003e\n      \u003cul class=\"posting_meta_section_list selected_user_list\"\u003e\u003c/ul\u003e\n\n      {{#if network.subGroups}}\n      \u003cdiv id=\"subgroup_list\" style='display:none;'\u003e\n        \u003cselect id=\"subgroup_dropdown\" onchange=\"P.old_new_post.subGroupChange()\"\u003e\n          {{#if network.showAllGroups}}\n          \u003coption value='All my groups'\u003eAll my groups\u003c/option\u003e\n          {{/if}}\n          {{#each network.subGroups}}\n          \u003coption value='{{this}}'\u003e{{this}}\u003c/option\u003e\n          {{/each}}\n        \u003c/select\u003e\n        \u003cdiv class=\"error_wrapper\" id=\"not-in-section-student\" style=\"display: none;\"\u003e\n          \u003cdiv class=\"error main\"\u003eYou are currently not in this group\u003c/div\u003e\n          \u003cdiv class=\"error sub\"\u003ePlease ask your instructor to enroll you to this group on Piazza\u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"error_wrapper\" id=\"not-in-section-can-join\" style=\"display: none;\"\u003e\n          \u003cdiv class=\"error main\"\u003eYou are currently not in this group \u003ca href=\"#\" onclick=\"return P.old_new_post.joinSection($('#subgroup_dropdown').val());\"\u003eJoin Group\u003c/a\u003e\u003c/div\u003e\n          \u003cdiv class=\"error sub\"\u003eYou have to join this group to post\u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"error_wrapper\" id=\"not-in-section-instructor\" style=\"display: none;\"\u003e\n          \u003cdiv class=\"error main\"\u003eYou are currently not in this group\u003c/div\u003e\n          \u003cdiv class=\"error sub\"\u003eYou will not see this post in your feed\u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      {{/if}}\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n\n\u003c!-- always_open: always_open, anonymous: anonymous, \n      revote: revote, multiple_selections: multiple_selections, bypass_email: bypass_email --\u003e\n\n\n\n\n  {{#unless network.config.disable_folders}}\n  \u003cdiv class=\"posting_meta_section select_folders clearFix\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003eSelect Folder(s)\u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      \u003cul id=\"new_folder_list\"\u003e\n        {{#each network.folders}}\n          {{#ifNotEquals this \"polls\"}}\u003cli data-pats=\"folders_item\"\u003e\u003ca data-pats=\"button\" class=\"tag folder {{#ifEquals ../../feed_filter this}}selected{{/ifEquals}}\" href=\"#\"\u003e{{this}}\u003c/a\u003e\u003c/li\u003e{{/ifNotEquals}}\n        {{/each}}\n      \u003c/ul\u003e\n      {{#if hasManyFolders}}\n      \u003ca class=\"show_more_folders\" href=\"#\"\u003eShow more folders\u003c/a\u003e\n      {{/if}}\n      \u003cdiv data-pats=\"folder_error_text\" class=\"new_post_error no_folders\"\u003ePlease select at least one folder for your post.\u003c/div\u003e\n      {{#if network.hasNoFolders}}\u003cdiv class=\"new_post_message\"\u003eYour class is currently set to use default folders. Edit these folders via Manage Class page.\u003c/div\u003e{{/if}}\n    \u003c/div\u003e\n  \u003c/div\u003e\n  {{/unless}}\n  \u003cdiv class=\"posting_meta_section post_summary clearFix\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003e\n      Summary\n      \u003csmall\u003e(100 characters or less)\u003c/smalL\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n    {{#if is_clone}}\u003cdiv class=\"error_message no_folders\"\u003eSince this is a cloned poll, be careful to change the folders or content so you will not have a duplicate poll.\u003c/div\u003e{{/if}}\n      \u003cinput type=\"text\" id=\"post_summary\" class=\"new_post_textarea\" placeholder=\"Enter a one line summary...\"/\u003e\n      \u003cdiv data-pats=\"summary_error_text\" class=\"new_post_error no_summary\"\u003ePlease enter a brief summary of your question.\u003c/div\u003e\n      \u003cdiv data-pats=\"summary_limit_error_text\" class=\"new_post_error long_summary\"\u003ePlease enter a summary with no more than 100 characters.\u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"posting_meta_section post_details clearFix\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003eDetails\u003cbr/\u003e\n      {{#ifEquals user.config.default_editor \"plain\"}}\n      \u003cdiv style=\"font-size: 11px;font-weight:normal;margin-top:5px;\"\u003e\u003ca href=\"#\" class=\"richtext_toggle\" onclick=\"return PEM.fire('toggleRichtext')\"\u003euse rich text editor\u003c/a\u003e\u003c/div\u003e\n      {{else}}\n      \u003cdiv style=\"font-size: 11px;font-weight:normal;margin-top:5px;\"\u003e\u003ca href=\"#\" class=\"richtext_toggle\" onclick=\"return PEM.fire('toggleRichtext')\"\u003euse plain text editor\u003c/a\u003e\u003c/div\u003e\n      {{/ifEquals}}\n    \u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      \u003cdiv id=\"old_new_post_editor\"\u003e\u003c/div\u003e\n      \u003c!-- \u003ctextarea id=\"post_details\" class=\"new_post_textarea\" placeholder=\"Add post details here...\"\u003e\u003c/textarea\u003e --\u003e\n      \u003cdiv class=\"new_post_error no_details\"\u003ePlease provide some details to better explain your question.\u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  \u003c!--\u003cdiv class=\"post_to posting_meta_section show_attachments clearFix\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003eAttachments\u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      \u003cbutton class=\"btn btn-small\" id=\"add_attachment\"\u003eBrowse...\u003c/button\u003e \u003cspan class=\"small_grey_text\"\u003e You can attach files up to 70mb.\u003c/span\u003e\n      \u003cul class=\"posting_meta_section_list attachment_list\" id=\"attachment_list\"\u003e\u003c/ul\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e--\u003e\n  \u003cdiv class=\"posting_meta_section show_for_poll poll_choices clearFix\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003ePoll Choices\u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      \u003cdiv class=\"input-append\"\u003e\n        \u003cinput class=\"span3\" id=\"input_poll_choice\" placeholder=\"Enter poll choices...\" type=\"text\" \u003e\n        \u003cbutton class=\"btn\" type=\"button\" id=\"add_poll_choice\"\u003eAdd\u003c/button\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"new_post_error no_poll_choices\"\u003ePlease enter poll choices before submitting...\u003c/div\u003e\n      \u003cul id=\"poll_options_preview\" class=\"poll_options\"\u003e      \n        {{#if poll_options}}\n        {{#each poll_options}}\n          \u003cli\u003e\n          \u003cspan class=\"poll_option\"\u003e{{this}}\u003c/span\u003e\n          \u003ci class=\"icon-remove\" onclick=\"return P.old_new_post.removePollChoice({{@index}});\"\u003e\u003c/i\u003e\n          \u003c/li\u003e\n        {{/each}}\n      {{/if}}\n      \u003c/ul\u003e\n\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n  \u003cdiv class=\"posting_meta_section post_summary clearFix show_for_poll\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003e\n      Poll Explanation\n      \u003csmall\u003e(correct answer, etc.)\u003c/smalL\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      \u003cinput type=\"text\" id=\"input_poll_explanation\" class=\"span3\" placeholder=\"Enter an explanation...\"/\u003e\u003cspan class=\"small_grey_text\"\u003eThis is optional, and it will be displayed when the poll closes.\u003c/span\u003e\n\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n\n\n\u003c!--   \u003cdiv class=\"posting_meta_section show_for_poll correct_answer clearFix\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003eCorrect Answer?\u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      \u003clabel class=\"radio one_line\"\u003e\n        \u003cinput type=\"radio\" name=\"correct_answer\" id=\"correct_answer_yes\" value=\"option1\" checked\u003e\n        Yes\n      \u003c/label\u003e\n      \u003clabel class=\"radio one_line\"\u003e\n        \u003cinput type=\"radio\" name=\"correct_answer\" id=\"correct_answer_no\" value=\"option2\"\u003e\n        No\n      \u003c/label\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n --\u003e\n  \u003cdiv class=\"posting_meta_section show_for_poll selections_allowed clearFix\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003ePoll Selection Type\u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      \u003clabel class=\"radio one_line\"\u003e\n        \u003cinput type=\"radio\" name=\"selections_allowed\" id=\"selections_allowed_one\" value=\"option1\" checked\u003e\n        One choice allowed\n      \u003c/label\u003e\n      \u003clabel class=\"radio one_line\"\u003e\n        \u003cinput type=\"radio\" name=\"selections_allowed\" id=\"selections_allowed_more\" value=\"option2\"\u003e\n        Multiple choices allowed\n      \u003c/label\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"posting_meta_section show_for_poll poll_close_date clearFix\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003ePoll Close Date\u003cbr/\u003e\n     \u003csmall\u003e(you will be emailed a report after the poll closes)\u003c/small\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      \u003clabel class=\"radio\"\u003e\n        \u003cinput type=\"radio\" name=\"poll_close_date\" id=\"poll_close_date_after\" value=\"option1\" {{#unless always_open}}checked{{/unless}}\u003e\u003c/input\u003e\n        \u003cspan\u003eAfter\u003c/span\u003e\n      \u003c/label\u003e\n      \u003cselect class=\"short\" id=\"poll_close_date_menu\"\u003e\n        \u003coption value=\"2\"\u003e2 hours\u003c/option\u003e\n        \u003coption value=\"6\"\u003e6 hours\u003c/option\u003e\n        \u003coption value=\"24\"\u003e1 day\u003c/option\u003e\n        \u003coption value=\"48\" selected=\"selected\"\u003e2 days\u003c/option\u003e\n        \u003coption value=\"168\"\u003e7 days\u003c/option\u003e\n      \u003c/select\u003e\n      \u003cspan class=\"small_grey_text\"\u003eYou can close the poll at any time (useful for in-class polls)\u003c/span\u003e\n      \u003clabel class=\"radio one_line\"\u003e\n        \u003cinput type=\"radio\" name=\"poll_close_date\" id=\"poll_close_date_never\" value=\"option2\" {{#if always_open}}checked{{/if}}\u003e\n        Always open\n      \u003c/label\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"posting_meta_section show_for_poll show_poll_results clearFix\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003eShow Results to Students\u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      \u003clabel class=\"radio one_line\"\u003e\n        \u003cinput type=\"radio\" name=\"show_poll_results\" id=\"show_poll_results_before_member_votes\" value=\"option1\" {{#ifEquals show_results \"\"}} checked {{/ifEquals}}\u003e\n        Before a student votes\n      \u003c/label\u003e\n      \u003clabel class=\"radio one_line\"\u003e\n        \u003cinput type=\"radio\" name=\"show_poll_results\" id=\"show_poll_results_after_member_votes\" value=\"option2\" {{#ifEquals show_results \"av\"}} checked {{/ifEquals}}\u003e\n        After a student votes\n      \u003c/label\u003e\n      \u003clabel class=\"radio one_line\"\u003e\n        \u003cinput type=\"radio\" name=\"show_poll_results\" id=\"show_poll_results_after_poll_closes\" value=\"option3\" {{#ifEquals show_results \"ac\"}} checked {{/ifEquals}}\u003e\n        After poll closes\n      \u003c/label\u003e\n      \u003clabel class=\"radio one_line\"\u003e\n        \u003cinput type=\"radio\" name=\"show_poll_results\" id=\"show_poll_results_keep_private\" value=\"option4\" {{#ifEquals show_results \"nn\"}} checked {{/ifEquals}}\u003e\n        Never show results to students        \n        \u003cspan class=\"small_grey_text\"\u003e(instructors will always have the option to view results at any time)\u003c/span\u003e\n      \u003c/label\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n    \u003cdiv class=\"posting_meta_section show_for_poll revotes clearFix\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003eRevotes allowed?\u003cbr/\u003e\n    \u003csmall\u003eVoters will be able to revote as many times as they want\u003c/small\u003e\n\u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      \u003clabel class=\"radio one_line\"\u003e\n        \u003cinput type=\"radio\" name=\"revotes\" id=\"revotes_yes\" value=\"option1\" {{#if revote }} checked {{/if}}\u003e\n        Yes\n      \u003c/label\u003e\n      \u003clabel class=\"radio one_line\"\u003e\n        \u003cinput type=\"radio\" name=\"revotes\" id=\"revotes_no\" value=\"option2\" {{#unless revote }}checked {{/unless}}\u003e\n        No\n      \u003c/label\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n\n\n\n  \u003cdiv class=\"posting_meta_section show_for_poll poll_anonymity clearFix\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003ePoll Anonymity\u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      {{#unless network.isNoVerify}}\n        \u003clabel class=\"radio one_line\"\u003e\n          \u003cinput type=\"radio\" name=\"poll_anonymity\" id=\"poll_anonymity_everyone\" value=\"option1\" {{#ifEquals anonymity \"no\"}} checked {{/ifEquals}}\n\u003e\n          Show names to everyone\n        \u003c/label\u003e\n        \u003clabel class=\"radio one_line\"\u003e\n          \u003cinput type=\"radio\" name=\"poll_anonymity\" id=\"poll_anonymity_students\" value=\"option1\" {{#ifEquals anonymity \"stud\"}} checked {{/ifEquals}}\u003e\n          Show names to instructors but not students\n        \u003c/label\u003e\n      {{/unless}}\n      \u003clabel class=\"radio one_line\"\u003e\n        \u003cinput type=\"radio\" name=\"poll_anonymity\" id=\"poll_anonymity_nobody\" value=\"option2\" {{#ifEquals anonymity \"yes\"}} checked {{/ifEquals}}\u003e\n        Don't show names to anyone\n        \u003cspan class=\"small_grey_text\"\u003e(the only option for large classes)\u003c/span\u003e\n      \u003c/label\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  {{#if anon}}\n  \u003cdiv class=\"posting_meta_section clearFix\" id=\"post_anonymity_wrapper\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003eShow my name as\u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      \u003cselect class=\"anonymity_selector\" id=\"new_post_anonymity\"\u003e\n        {{#each anon}}\n        \u003coption value=\"{{this.val}}\"\u003e{{this.title}}\u003c/option\u003e\n        {{/each}}\n      \u003c/select\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  {{/if}}\n\n  \n  \u003cdiv class=\"posting_meta_section publish_to clearFix show_for_poll\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003eVisible to class\u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      \u003clabel class=\"radio\"\u003e\n        \u003cinput type=\"radio\" name=\"publish_to\" id=\"publish_now\" value=\"option1\" {{#unless publish_later}} checked {{/unless}}\u003e\n        Visible now\n      \u003c/label\u003e\n      \u003clabel class=\"radio\"\u003e\n        \u003cinput type=\"radio\" name=\"publish_to\" id=\"publish_to_individuals\" value=\"option2\"\u003e\n        Visible now to individuals\n      \u003c/label\u003e\n\n      \u003clabel class=\"radio\"\u003e\n      \u003cinput type=\"radio\" name=\"publish_to\" id=\"publish_later\" value=\"option3\" {{#if publish_later}} checked {{/if}}\u003e\n        Make visible later \n        \u003cspan class=\"small_grey_text\"\u003eYou can publish it later at any time to make it visible to the class\u003c/span\u003e\n\n      \u003c/label\u003e\n\n      \u003cdiv class=\"new_post_error too_many_names\"\u003eYou can send individual posts to up to 10 people\u003c/div\u003e\n      \u003cinput id=\"poll_member_names\" class=\"new_post_textarea\" type=\"text\" placeholder=\"Enter one or more names...\" style=\"display: none;\"\u003e\n      \u003cdiv id=\"poll_member_names_message\" class=\"small_grey_text\" style=\"display: none\"\u003eType \"Instructors\" to include all instructors.\u003c/div\u003e\n      \u003cdiv class=\"new_post_error no_names\"\u003eEnter at least one name before submitting this post\u003c/div\u003e\n      \u003cul class=\"dropdown-menu user_list general_dropdown\" id=\"poll_member_names_dropdown_list\" role=\"menu\" aria-labelledby=\"dropdownMenu\"\u003e\u003c/ul\u003e\n      \u003cul class=\"posting_meta_section_list poll_selected_user_list\"\u003e\u003c/ul\u003e\n\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n\u003c!--   \u003cdiv class=\"posting_meta_section publish_to clearFix show_for_poll\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003eVisible to class\u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      \u003clabel class=\"radio\"\u003e\n        \u003cinput type=\"radio\" name=\"publish_to\" id=\"publish_now\" value=\"option1\" {{#unless publish_later}} checked {{/unless}}\u003e\n        Make visible now\n      \u003c/label\u003e\n\n      \u003clabel class=\"radio\"\u003e\n      \u003cinput type=\"radio\" name=\"publish_to\" id=\"publish_later\" value=\"option2\" {{#if publish_later}} checked {{/if}}\u003e\n      Make visible later \n        \u003cspan class=\"small_grey_text\"\u003eYou can publish it later at any time to make it visible to the class\u003c/span\u003e\n\n      \u003c/label\u003e\n      \n    \u003c/div\u003e\n  \u003c/div\u003e --\u003e\n\n  {{#if isInst}}\n  \u003cdiv id=\"bypassing_email\" class=\"posting_meta_section posting_options clearFix\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003ePosting Options\u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      {{#if network.isLargeClass}}\n      \u003clabel class=\"checkbox one_line\" id=\"no_email_wrapper\"\u003e\n        \u003cinput type=\"checkbox\" name=\"posting_options\" id=\"posting_options_no_email\" value=\"option1\"\u003e\n        Disable alerts about this post for \u003ci\u003eeveryone\u003c/i\u003e \n        \u003cspan class=\"small_grey_text\"\u003e(Not recommended.)\u003c/span\u003e\n      \u003c/label\u003e\n      {{/if}}\n      \u003clabel class=\"checkbox one_line\" id=\"announcement_wrapper\"\u003e\n        \u003cinput type=\"checkbox\" name=\"posting_options\" id=\"posting_options_announcement\" value=\"option1\" onclick=\"P.old_new_post.isAnnouncement();\"\u003e\n        Make this an announcement\n        \u003cspan class=\"small_grey_text\"\u003e(note appears on the course page)\u003c/span\u003e\n      \u003c/label\u003e\n      \u003clabel class=\"checkbox one_line\" id=\"bypass_email\"\u003e\n        \u003cinput type=\"checkbox\" name=\"posting_options\" id=\"posting_options_bypass_email\" value=\"option1\" {{#if bypass_email}} checked {{/if}}\u003e\n        Send email notifications immediately\n        \u003cspan class=\"small_grey_text\"\u003e(bypassing {{#if network.isGroup}}members'{{else}}students'{{/if}} email preferences, if necessary)\u003c/span\u003e\n      \u003c/label\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n\u003c!--   \u003cdiv id=\"email_disabled\" class=\"posting_meta_section posting_options clearFix\"\u003e\n    \u003cdiv class=\"left_section main_label\"\u003eEmail Options\u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n      \u003clabel class=\"checkbox one_line\" id=\"email_wrapper\"\u003e\n        \u003cinput type=\"checkbox\" name=\"posting_options\" id=\"posting_options_email\" value=\"option1\"\u003e\n        Disable receiving email notifications\n      \u003c/label\u003e\n      \u003clabel class=\"checkbox one_line\"\u003e\n        \u003cinput type=\"checkbox\" name=\"posting_options\" id=\"posting_options_bypass_email\" value=\"option1\" {{#if email_disabled}} checked {{/if}}\u003e\n        Send email notifications immediately\n        \u003cspan class=\"small_grey_text\"\u003e(class members will not be notified of a poll creation)\u003c/span\u003e\n      \u003c/label\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e --\u003e\n  {{/if}}\n  \u003cdiv class=\"posting_meta_section posting_actions selections_allowed clearFix\"\u003e\n    \u003cdiv class=\"left_section spacer\"\u003e\u003c/div\u003e\n    \u003cdiv class=\"right_section\"\u003e\n        \u003cbutton class=\"btn btn-warning\" id=\"post_button\"\u003ePost My Question!\u003c/button\u003e\n        \u003cbutton class=\"btn\" id=\"save_draft\"\u003eSave Draft\u003c/button\u003e\n        \u003cbutton class=\"btn\" id=\"post_cancel_button\" onclick=\"dataLayer.push({'event': 'eventTracker', 'eventCat': 'Post', 'eventAct': 'Cancel', 'eventLbl': '', 'eventVal': 0}); \"\u003eCancel\u003c/button\u003e\n      \u003cdiv class=\"new_post_actions\"\u003e\n        \u003ca href=\"#\" class=\"preview_button\" onclick=\"return PEM.fire('previewPost', 'old_new_post')\" style=\"display: block; margin-top: 5px; float: left;\"\u003ePreview Post\u003c/a\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n");</script>

<script type="text/javascript">
P.modules.loadModule("history_slider", {}, null, "  \u003cdiv id=\"history_slider\"\u003e\n    \u003cdiv id=\"question_history_slider_wrapper\" class=\"dashboard_toolbar\"\u003e\n      \u003cdiv class=\"question_history_slider_label\"\u003e\u003cspan class=\"content_type_title_history\"\u003e{{type}}\u003c/span\u003e History:\u003c/div\u003e\n      {{#if haveHistory}}\n      \u003cdiv class=\"slider_wrapper clearfix\"\u003e\n        \u003cdiv class=\"slider slider-horizontal\" id=\"actual_history_slider\"\u003e\u003c/div\u003e\n      \u003c/div\u003e\n      {{else}}\n        {{#if content.disabledHistory}}\n         \u003cdiv class=\"question_history_slider_label\" id=\"question_history_slider_no_history\"\u003eHistory Disabled By Instructor\u003c/div\u003e\n        {{else}}\n         \u003cdiv class=\"question_history_slider_label\" id=\"question_history_slider_no_history\"\u003eNo history yet\u003c/div\u003e\n        {{/if}}\n      {{/if}}\n    \u003c/div\u003e\n    \u003cdiv id=\"question_history_description_wrapper\" style=\"display:none;\"\u003e\n      \u003cdiv id=\"question_history_description\" class=\"clearfix\"\u003e\n        \u003cdiv id=\"question_history_image\" class=\"user_pic\"\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"content\"\u003e\n          \u003cspan id=\"question_history_data\"\u003e\n          \u003c/span\u003e\n          \u003cspan id=\"question_history_timestamp\"\u003e24 minutes ago\u003c/span\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n");</script>

<script type="text/javascript">
P.modules.loadModule("special_mentions", {}, null, "\u003cdiv id=\"special_mentions\" class=\"dashboard_toolbar special_mentions_wrapper\"\u003e\n  \u003cdiv id=\"special_mentions_techtour\" class=\"tech_tour_hook\"\u003e\n    \u003ctable class=\"tech_tour_hook_table\"\u003e\n      \u003ctr\u003e\n        \u003ctd colspan=\"2\" class=\"tech_tour_title\"\u003e\n          See highlights from our \u003cb\u003ebehind-the-scenes\u003c/b\u003e Silicon Valley Tech Tour last year!\n          \u003ca href=\"#\" class=\"tt_dismiss_link\" onclick=\"P.special_mentions.dismissTechTour()\"\u003edismiss\u003c/a\u003e\n        \u003c/td\u003e\n        \u003ctd class=\"tech_tour_video_link\" href=\"#tech_tour_video_modal\" role=\"button\" data-toggle=\"modal\" onclick=\"P.special_mentions.openTechTourVideo()\" \u003e\n          \u003ca href=\"#tech_tour_video_modal\" role=\"button\" data-toggle=\"modal\"\u003e\n            \u003cimg class=\"play_btn_screen\"\u003e\n            \u003cdiv class=\"play_btn\"\u003e▶\u003c/div\u003e\n          \u003c/a\u003e\n        \u003c/td\u003e\n        \u003c!-- \u003ctd class=\"tt_image_cell\"\u003e\n          \u003cdiv class=\"tt_image first_image\"\u003e\u003c/div\u003e\n        \u003c/td\u003e\n        \u003ctd class=\"tt_image_cell\"\u003e\n          \u003cdiv class=\"tt_image second_image\"\u003e\u003c/div\u003e\n        \u003c/td\u003e\n        \u003ctd class=\"tt_image_cell\"\u003e\n          \u003cdiv class=\"tt_image third_image\"\u003e\u003c/div\u003e\n        \u003c/td\u003e --\u003e\n        \u003ctd class=\"tt_button_cell\"\u003e\n          \u003ca target=\"_blank\" href=\"/techtour\" class=\"btn btn-default tt_apply_button\"\u003eApply for the Summer Tech Tour\u003c/a\u003e\n        \u003c/td\u003e\n      \u003c/tr\u003e\n    \u003c/table\u003e\n\u003c/div\u003e\n  \u003cdiv id=\"special_mentions_regular\" style=\"display: none;\"\u003e\n    \u003cdiv class=\"stats_item left\"\u003e\n      \u003ch3 class=\"ellipses\"\u003eAverage Res