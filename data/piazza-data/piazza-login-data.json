PTM.createModuleTemplate("homescreen", "homescreen_new_careers_feed", "\u003cli class=\"company_story clearfix\" id=\"{{item.id}}\"\u003e\n  \u003cdiv class=\"company_logo homescreen_company_logo\"\u003e\n    \u003ca href=\"/careers/dashboard?company={{item.config.company_info.internal_id}}\"\u003e\u003cimg src=\"{{item.config.company_info.image}}\"\u003e\u003c/a\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"company_content_wrapper\"\u003e\n    {{{item.body}}}\n    {{#if item.config.external_link}}\n    \u003ca href=\"{{item.config.external_link}}\" class=\"read_more_link\" target=\"_blank\"\u003eRead More \u0026#187;\u003c/a\u003e\n    {{else}}\n    \u003ca href=\"/careers/dashboard?company={{item.config.company_info.internal_id}}{{#if item.config.section}}\u0026section={{item.config.section}}{{/if}}\" class=\"read_more_link\"\u003e{{#if item.config.video}}Watch Video{{/if}}{{#unless item.config.video}}Read More{{/unless}} \u0026#187;\u003c/a\u003e\n    {{/if}}\n  \u003c/div\u003e\n\u003c/li\u003e");</script>

  <script type="text/javascript">
PTM.createModuleTemplate("homescreen", "homescreen_tech_tour", "\u003cdiv class=\"homescreen homescreen_careers clearfix\"\u003e\n  \u003cdiv class=\"homescreen_box homescreen_box_tech_talk\"\u003e\n    \u003cdiv class=\"row\"\u003e\n      \u003cdiv class=\"span12\"\u003e\n        \u003cdiv class=\"tt_logo\"\u003e\u003c/div\u003e\n        \u003ch1 class=\"tech_tour_main_header\"\u003e\n          See highlights from our \u003cb\u003ebehind-the-scenes\u003c/b\u003e Silicon Valley Tech Tour last year!\n        \u003c/h1\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003c!-- \u003cdiv class=\"row\"\u003e --\u003e\n      \u003c!-- \u003cdiv class=\"span12\"\u003e --\u003e\n        \u003cdiv class=\"tech_tour_cover\"\u003e\n          \u003c!-- \u003cimg src=\"https://s3.amazonaws.com/piazza-uploads/careers/tech_tour/tech_tour_cover.jpg\" alt=\"\" /\u003e --\u003e\n          \u003cdiv class=\"tech_tour_video\"\u003e\n            \u003ciframe width=\"650\" height=\"365\" src=\"https://www.youtube.com/embed/qVuprSMlZ38?rel=0\" frameborder=\"0\" allowfullscreen\u003e\u003c/iframe\u003e\n            \u003c!-- \u003cdiv class=\"tech_tour_footer\"\u003eSee highlights from our Summer Tech Tour\u003c/div\u003e --\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c!-- \u003c/div\u003e --\u003e\n    \u003c!-- \u003c/div\u003e --\u003e\n    \n    \n    \u003cdiv class=\"row need_upload_resume\"\u003e\n      \u003cdiv class=\"text-center\"\u003e\n        \u003ca href=\"/techtour\" class=\"btn btn-large new_apply_button\" target=\"_blank\"\u003eApply for the Summer Tech Tour\u003c/a\u003e\n          \u003c!-- \u003cbutton id=\"upload_resume_button\" class=\"btn btn-large new_apply_button\"\u003e\n          Upload your resume to get started\n        \u003c/button\u003e\n        \u003cform method=\"POST\" id=\"upload_form\" class=\"hide\" action=\"/careers/upload_resume\" enctype=\"multipart/form-data\" target=\"hidden-upload-frame\"\u003e\n          \u003cinput type=\"file\" id=\"upload_form_file\" name=\"file\" onchange=\"P.homescreen.startUpload();\"/\u003e\n        \u003c/form\u003e\n        \u003ciframe id=\"hidden-upload-frame\" name=\"hidden-upload-frame\" style=\"display: none\" onload=\"P.homescreen.onUploadComplete();\"\u003e\u003c/iframe\u003e --\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"row no_need_upload_resume\" style=\"display:none;\"\u003e\n      \u003cdiv class=\"span6\"\u003e\n        \u003ch1 class=\"text-center\"\u003e\n            We already have your resume\n        \u003c/h1\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"span6\"\u003e\n        \u003cp class=\"text-center\"\u003e\n          \u003cbutton class=\"btn btn-large btn-success btn-block apply_button\"\u003e\n            Apply\n          \u003c/button\u003e\n        \u003c/p\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"row sv2014_apply_done\" style=\"display:none;\"\u003e\n      \u003cdiv class=\"span12\"\u003e\n        \u003ch1 class=\"text-center\"\u003e\n            Thanks for applying!\n        \u003c/h1\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"clearfix\"\u003e\n      \u003ca href=\"#terms_modal\" data-toggle=\"modal\" class=\"pull-left\"\u003e\n        *Terms and Conditions Apply\n      \u003c/a\u003e\n      \u003ca href=\"#\" class=\"pull-right tech_tour_dismiss\" onclick=\"P.homescreen.hideTechTours()\"\u003e\n        Dismiss\n      \u003c/a\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n\n\u003cdiv id=\"terms_modal\" class=\"modal hide fade\" tabindex=\"-1\" role=\"dialog\"\u003e\n  \u003cdiv class=\"modal-header\"\u003e\n    \u003cbutton type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-hidden=\"true\"\u003e×\u003c/button\u003e\n    \u003ch4\u003eTerms and Conditions for Piazza Careers Tech Tours\u003c/h4\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"modal-body\"\u003e\n    \u003cp\u003e\n      The Piazza Careers Tech Tours are a once-in-a-lifetime opportunity for current students to enjoy a behind-the-scenes tour of the world's most innovative tech companies.\n    \u003c/p\u003e\n    \u003ch3\u003e\n      Eligibility Requirements\n    \u003c/h3\u003e\n    \u003cul\u003e\n      \u003cli\u003e\n        Must be an active, full-time undergraduate or graduate student at an accredited university in the United States or Canada\n      \u003c/li\u003e\n      \u003cli\u003e\n        Must be enrolled in a class on Piazza\n      \u003c/li\u003e\n    \u003c/ul\u003e\n    \u003ch3\u003e\n      Application process\n    \u003c/h3\u003e\n    \u003cul\u003e\n      \u003cli\u003e\n        Student must submit an updated professional resume and complete all profile questions on the submission page.\n      \u003c/li\u003e\n      \u003cli\u003e\n        Tours will include private meals, tech talks and meet and greets with prominent engineers and product managers.\n      \u003c/li\u003e\n      \u003cli\u003e\n        Piazza will cover airfare, hotel, meals, and transportation to and from the airport. All other incidental expenses will be the responsibility of the tour invitee.\n      \u003c/li\u003e\n    \u003c/ul\u003e\n    \u003ch3\u003e\n      Deadlines\n    \u003c/h3\u003e\n    \u003cul\u003e\n      \u003cli\u003e\n        Applications are due April 30th, 2018 at 9am PST.\n      \u003c/li\u003e\n      \u003cli\u003e\n        Invitees will be notified by June 30th, 2018 at 9am PST.\n      \u003c/li\u003e\n    \u003c/ul\u003e\n    \u003ch3\u003e\n      Tour dates\n    \u003c/h3\u003e\n    \u003cul\u003e\n      \u003cli\u003e\n        Tour dates will be confirmed when invitations are extended to the selected students. If a selected student cannot make the tour date provided, then the spot will be extended to another applicant.\n      \u003c/li\u003e\n    \u003c/ul\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n");</script>

  <script type="text/javascript">
PTM.createModuleTemplate("careers_dashboard", "careers_dashboard_content_story", "\u003cli class=\"company_story clearfix\" id=\"{{item.id}}\"\u003e\n  \u003cdiv class=\"company_logo homescreen_company_logo\"\u003e\n    \u003ca href=\"#\" class=\"company_link\" company=\"{{item.config.company_info.internal_id}}\"\u003e\u003cimg src=\"{{item.config.company_info.image}}\"\u003e\u003c/a\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"company_content_wrapper\"\u003e\n    {{{item.body}}}\n    {{#if item.config.external_link}}\n    \u003ca href=\"{{item.config.external_link}}\" class=\"read_more_link\" target=\"_blank\"\u003eRead More \u0026#187;\u003c/a\u003e\n    {{else}}\n    \u003ca href=\"#\" {{#if item.config.section}}section=\"{{item.config.section}}\"{{/if}} company=\"{{item.config.company_info.internal_id}}\" class=\"read_more_link company_link\" target=\"_blank\"\u003e{{#if item.config.video}}Watch Video{{/if}}{{#unless item.config.video}}Read More{{/unless}} \u0026#187;\u003c/a\u003e\n    {{/if}}\n  \u003c/div\u003e\n\u003c/li\u003e\n");</script>

  <script type="text/javascript">
PTM.createModuleTemplate("careers_dashboard", "careers_dashboard_event_story", "\u003cli class=\"company_story clearfix\" id=\"{{item.id}}\" school=\"{{this.school}}\" company=\"{{item.config.company_info.internal_id}}\"\u003e\n  \u003cdiv class=\"company_logo homescreen_company_logo\"\u003e\n    \u003ca href=\"#\" class=\"company_link\" company=\"{{item.config.company_info.internal_id}}\"\u003e\u003cimg src=\"{{item.config.company_info.image}}\"\u003e\u003c/a\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"company_content_wrapper\"\u003e\n    \u003ch2\u003e{{{item.body}}}\u003c/h2\u003e\n    \u003cdiv class=\"company_content clearfix\"\u003e\n      \u003cdiv class=\"clearfix\"\u003e\n        \u003cdiv class=\"company_event_blurb\"\u003e\n          \u003cp\u003e{{{item.config.event_info.summary}}}\u003c/p\u003e\n        \u003c/div\u003e\n        \u003cul class=\"event_info clearfix\"\u003e\n          {{#if item.config.event_info.date}}\n          \u003cli class=\"clearfix\"\u003e\n            \u003ci class=\"icon-calendar\"\u003e\u003c/i\u003e\n            \u003cspan\u003e{{item.config.event_info.date}}\u003c/span\u003e\n          \u003c/li\u003e\n          {{/if}}\n          {{#if item.config.event_info.time}}\n          \u003cli class=\"clearfix\"\u003e\n            \u003ci class=\"icon-time\"\u003e\u003c/i\u003e\n            \u003cspan\u003e{{item.config.event_info.time}}\u003c/span\u003e\n          \u003c/li\u003e\n          {{/if}}\n          {{#if item.config.event_info.where}}\n          \u003cli class=\"clearfix\"\u003e\n            \u003ci class=\"icon-map-marker\"\u003e\u003c/i\u003e\n            \u003cspan\u003e{{item.config.event_info.where}}\u003c/span\u003e\n          \u003c/li\u003e\n          {{/if}}\n        \u003c/ul\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"follow_actions_wrapper clearfix\"\u003e\n        {{#if item.config.following}}\n        \u003cdiv class=\"stop_follow_wrapper\"\u003e\n          \u003cdiv class=\"clearfix\"\u003e\n            \u003cbutton class=\"btn btn-primary btn-small do-unfollow-event\" event=\"{{item.id}}\" company=\"{{item.config.company_info.internal_id}}\"\u003eStop Following This Event\u003c/button\u003e\n            \u003cspan\u003eYou are currently \u003cbr\u003efollowing this event.\u003c/span\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"event-like-cd\"\u003e\u003c/div\u003e\n        \u003c/div\u003e\n        {{else}}\n        \u003cdiv class=\"follow_wrapper\"\u003e\n          \u003cdiv class=\"clearfix\"\u003e\n            \u003cbutton class=\"btn btn-primary btn-small do-follow-event\" event=\"{{item.id}}\" cname=\"{{item.config.company_info.name}}\" company=\"{{item.config.company_info.internal_id}}\"\u003eFollow This Event\u003c/button\u003e\n            \u003cspan\u003eFollowing this event keeps you \u003cbr\u003eposted with event updates.\u003c/span\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"event-like-cd\"\u003e\u003c/div\u003e\n        \u003c/div\u003e\n        {{/if}}\n      \u003c/div\u003e\n      {{#if item.config.followers}}\n      \u003cdiv class=\"follower_number clearfix\"\u003e\n        \u003ci class=\"icon-user\"\u003e\u003c/i\u003e\n        \u003cspan\u003e{{item.config.followers}} student(s) following this event\u003c/span\u003e\n      \u003c/div\u003e\n      {{/if}}\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/li\u003e");</script>

