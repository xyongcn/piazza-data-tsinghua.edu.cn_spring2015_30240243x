<script type="text/javascript">
P.modules.loadModule("top_bar", {}, null, "\u003cdiv class=\"navbar top_bar\" id=\"top_bar\"\u003e\n\n  \u003cdiv class=\"job_dropdown_wrapper\" id=\"job_status_dropdown\" style=\"display: none;\" onclick=\"P.top_bar.clickJobStatus()\"\u003e\n    \u003ch4\u003eVerify your job status for \u003cspan id=\"job_year\"\u003e\u003c/span\u003e:\u003c/h4\u003e\n    \u003ch4 class=\"info_text\"\u003eLet companies know so they can message (or not message) you accordingly.\u003c/h4\u003e\n    \u003cul\u003e\n      \u003cli\u003e\u003cinput type=\"radio\" id=\"ft\" name=\"job_status\" value=\"seeking_full\"\u003e\u0026nbsp;\u0026nbsp;\u003clabel for=\"ft\"\u003eSeeking Full-Time\u003c/label\u003e\u003c/li\u003e\n      \u003cli\u003e\u003cinput type=\"radio\" id=\"int\" name=\"job_status\" value=\"seeking_intern\"\u003e\u0026nbsp;\u0026nbsp;\u003clabel for=\"int\"\u003eSeeking Internship\u003c/label\u003e\u003c/li\u003e\n      \u003cli\u003e\u003cinput type=\"radio\" id=\"acp\" name=\"job_status\" value=\"accepted\"\u003e\u0026nbsp;\u0026nbsp;\u003clabel for=\"acp\"\u003eAccepted Position\u003c/label\u003e\u003c/li\u003e\n      \u003cli\u003e\u003cinput type=\"radio\" id=\"na\" name=\"job_status\"\u003e\u0026nbsp;\u0026nbsp;\u003clabel for=\"na\"\u003eN/A\u003c/label\u003e\u003c/li\u003e\n    \u003c/ul\u003e\n    \u003cbutton class=\"btn btn-success update_job_status_button\" onclick=\"P.top_bar.updateJobStatus()\"\u003eUpdate\u003c/button\u003e\n    \u003cdiv class=\"timer\"\u003e\u003cspan class=\"time_left\" id='job_status_timer'\u003e\u003c/span\u003e\u003c/div\u003e\n  \u003c/div\u003e\n\n  \n  \n  \u003cdiv class=\"job_dropdown_wrapper\" style=\"display: none;\" id=\"upload_resume_dropdown\"\u003e\n    \u003ch4\u003eRecruiters are scheduling interviews now - upload your resume:\u003c/h4\u003e\n    \u003ch4 class=\"info_text main_info_text\"\u003eRecruiting is in full swing, employers are reaching out today to schedule interviews with students like you.\u003c/h4\u003e\n    \u003ch4 class=\"info_text sub_info_text\"\u003e\u003ca href=\"/careers/dashboard#/my_profile\" target=\"_blank\" onclick=\"P.top_bar.clickUpdateFromResumeDropdown()\"\u003eUpdate your profile\u003c/a\u003e to be found.\u003c/h4\u003e\n    \u003cdiv class=\"close_resume_dropdown\"\u003e\n      \u003ci class=\"glyphicon glyphicon-remove icon-remove\" onclick=\"P.top_bar.dismissResumeDropdown()\"\u003e\u003c/i\u003e\n    \u003c/div\u003e\n    \u003c!-- \u003cbutton class=\"btn btn-success update_job_status_button\" onclick=\"P.top_bar.updateJobStatus()\"\u003eUpdate\u003c/button\u003e --\u003e\n    \u003c!-- \u003cdiv class=\"timer\"\u003e\u003cspan class=\"time_left\" id='job_status_timer'\u003e\u003c/span\u003e\u003c/div\u003e --\u003e\n  \u003c/div\u003e\n\n  \u003cdiv class=\"share_job_wrapper clearfix\" id=\"job_details_dropdown\" style=\"display:none;\"\u003e\n    \u003ch5\u003eTell classmates where you're headed:\u003c/h5\u003e\n    \u003cul\u003e\n      \u003cli\u003e\u003clabel for=\"company\" class=\"company_name_input\"\u003eCompany\u003c/label\u003e\u003cinput id=\"company\"\u003e\u003c/li\u003e\n      \u003cli\u003e\u003clabel for=\"role\" class=\"company_role_input\"\u003eRole\u003c/label\u003e\u003cinput id=\"role\"\u003e\u003c/li\u003e\n      \u003cli\u003e\u003clabel for=\"location\" class=\"company_location_input\"\u003eLocation (State/Country/Closest City)\u003c/label\u003e\u003cinput id=\"location\"\u003e\u003c/li\u003e\n    \u003c/ul\u003e\n    \u003cbutton class=\"btn btn-success update_job_status_button pull-right\" onclick=\"P.top_bar.updateHeadedTo()\"\u003eUpdate\u003c/button\u003e\n    \u003cp class=\"does_not_apply pull-left\" onclick=\"P.top_bar.hideJobDetails()\"\u003edoesn't apply to me\u003c/p\u003e\n  \u003c/div\u003e\n\n  \u003cimg src=\"/images/piazza/dashboard/waitlist_modal_screenshots.png\" style=\"display: none;\" id=\"hidden_waitlist_img\"/\u003e\n  \u003cdiv class=\"navbar-inner\"\u003e\n    \u003ca id=\"classes_brand\" class=\"brand logo class_logo\" href=\"#\"\u003e\u003c/a\u003e\n    \u003ca id=\"careers_brand\" class=\"brand logo careers_logo\" href=\"#/companies\" style=\"display:none;\"\u003e\u003c/a\u003e\n    {{#unless hasNoClasses}}\n    \u003cul class=\"nav navbar-nav\"\u003e\n      \u003cli class=\"dropdown class_list\"\u003e\n        \u003ca href=\"#\" class=\"dropdown-toggle\" id=\"network_button\"\u003e\n          \u003cspan id=\"topbar_current_class_number\" class=\"current_class_name\"\u003e\u003c/span\u003e\n          \u003cspan class=\"caret\"\u003e\u003c/span\u003e\n          \u003cspan class=\"top_bar_notification\" id=\"global_notifications_indicator_wrapper\" style=\"display:none;\"\u003e\u003cdiv id=\"global_notifications_indicator\" style=\"font-weight:bold;\"\u003e\u003c/div\u003e\u003c/span\u003e\n        \u003c/a\u003e\n      \u003c/li\u003e\n      \u003cli class=\"top_bar_tab top_bar_qa\" {{#unless id}}style=\"display: none;\"{{/unless}}\u003e\n        \u003ca href=\"#\"\u003e\n          Q \u0026amp; A\n        \u003c/a\u003e\n      \u003c/li\u003e\n      \u003cli class=\"top_bar_tab top_bar_classmates\" style=\"display:none;\"\u003e\n        \u003ca href=\"#\" class=\"careers_dashboard_link\"\u003e\n          Classmates\n        \u003c/a\u003e\n      \u003c/li\u003e\n      \u003cli class=\"top_bar_tab top_bar_course_page\"\u003e\n        \u003ca href=\"#\"\u003e\n          Resources\n        \u003c/a\u003e\n      \u003c/li\u003e\n      \u003cli class=\"top_bar_tab top_bar_statistics topbar_icons stats\" style=\"display:none;\"\u003e\n        \u003ca href=\"#\" data-pats=\"statistics_link\"\u003e\n          Statistics\n        \u003c/a\u003e\n      \u003c/li\u003e\n      \u003cli class=\"top_bar_tab top_bar_manage\" style=\"display:none;\"\u003e\n        \u003ca href=\"#\"\u003e\n          Manage Class\n        \u003c/a\u003e\n      \u003c/li\u003e\n      \u003cli id=\"signup_button\" class=\"top_bar_tab top_bar_signup\" style=\"display: none;\"\u003e\n        \u003ca href=\"#\"\u003e\n          Sign up in 2 mins. It's free!\n        \u003c/a\u003e\n      \u003c/li\u003e\n    \u003c/ul\u003e\n    \u003cdiv class=\"top_bar_dropdown_wrapper\" style=\"display: none;\"\u003e\n      \u003cdiv id=\"my_class_menu\" class=\"top_bar_dropdown_menu\"\u003e\n        \u003cul id=\"my_classes\"\u003e\n        \u003c/ul\u003e\n        \u003cul id=\"inactive_network_toggle\" style=\"padding:0px;\"\u003e\u003cli\u003e\u003ca href=\"#\" style=\"color:#3c7cc0\"\u003eShow inactive classes\u003c/a\u003e\u003c/li\u003e\u003c/ul\u003e\n        \u003cdiv class=\"top_bar_dropdown_actions_wrapper clearFix instructor_only\"\u003e\n          \u003ca class=\"top_bar_dropdown_actions\" id=\"create_new_class\" href=\"#\"\u003eCreate a New Class...\u003c/a\u003e\n          \u003ca class=\"top_bar_dropdown_actions\" id=\"clone_class\" href=\"#\"\u003eClone This Class...\u003c/a\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"top_bar_dropdown_actions_wrapper clearFix student_only\"\u003e\n          \u003ca class=\"top_bar_dropdown_actions\" href=\"#\" id=\"add_another_class\"\u003e+\u0026nbsp;Add Another Class\u003c/a\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    {{/unless}}\n    \u003cul class=\"nav clearfix pull-right\"\u003e\n      \u003cli\u003e\n        \u003ca href=\"/careers/dashboard#/my_profile\" target=\"_blank\" class=\"account_name_container\" onclick=\"return P.top_bar.nameClick();\"\u003e\n          \u003cspan class=\"my_pic\"\u003e\u003c/span\u003e\n          \u003cspan class=\"my_name_wrapper \"\u003e\n            \u003cspan class=\"my_name\"\u003e{{name}}\u003c/span\u003e\n            \n            \u003cbr\u003e\n            \u003c!--\u003cspan style=\"display: none;\" class=\"job_link\" onclick=\"return P.top_bar.maybeShowPermanentEmail(true);\"\u003eKeep Piazza access\u003c/span\u003e--\u003e\n            \u003cspan style=\"display: none;\" class=\"job_link\" onclick=\"window.open('/about/jobs', '_blank');return false;\"\u003eJoin our team\u003c/span\u003e\n          \n          \u003c/span\u003e\n        \u003c/a\u003e\n      \u003c/li\u003e\n      \u003cli class=\"dropdown careers_dropdown\"\u003e\n        \u003ca href=\"#\" class=\"dropdown-toggle\" id=\"user_button\"\u003e\u003ci class=\"glyphicon glyphicon-cog icon-cog icon-white\"\u003e\u003c/i\u003e\u003c/a\u003e\n      \u003c/li\u003e\n    \u003c/ul\u003e\n    \u003cdiv class=\"top_bar_dropdown_wrapper\" style=\"display: none;\"\u003e\n      \u003cdiv id=\"user_dropdown\" class=\"top_bar_dropdown_menu\" style=\"top:32px;\"\u003e\n        \u003cul id=\"user_dropdown_list\"\u003e\n          \u003cli action=\"settings\"\u003e\u003ca id=\"account_settings_link\" href=\"/account_settings\"\u003eAccount/Email Settings\u003c/a\u003e\u003c/li\u003e\n          \u003cli action=\"tooltips\"\u003e\u003ca href=\"#\" id=\"tooltip_link\"\u003e\n            {{#if config.no_tips}}\n            Turn on Tooltips\n            {{else}}\n            Turn off Tooltips\n            {{/if}}\n          \u003c/a\u003e\u003c/li\u003e\n          \u003cli action=\"support\"\u003e\u003ca href=\"https://piazza.desk.com/\" target=\"_blank\"\u003eSupport\u003c/a\u003e\u003c/li\u003e\n          \u003cli action=\"bug\" id=\"report_bug_item\"\u003e\u003ca class=\"reportBug\" href=\"#\"\u003eReport Bug\u003c/a\u003e\u003c/li\u003e\n          \u003cli action=\"homepage\"\u003e\u003ca href=\"/signup\"\u003ePiazza Homepage\u003c/a\u003e\u003c/li\u003e\n        \u003c/ul\u003e\n        \u003cdiv action=\"logout\" class=\"top_bar_dropdown_actions_wrapper clearFix\"\u003e\n          \u003ca class=\"top_bar_dropdown_actions\" id=\"log_out\" href=\"#\"\u003eLog Out\u003c/a\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cul class=\"nav clearfix pull-right dark_nav top_bar_careers_homescreen\" style=\"display:none;\"\u003e\n      \u003cli class=\"careers_intro_modal clearfix\" style=\"display:none;\"\u003e\n        \u003cdiv class=\"careers_onboarding_arrow\"\u003e\u003c/div\u003e\n        \u003cdiv class=\"clearfix close_careers_intro_modal\"\u003e\n          \u003ci class=\"glyphicon glyphicon-remove icon-remove careers_intro_close\"\u003e\u003c/i\u003e\n        \u003c/div\u003e\n        \u003ch2\u003e\n          Introducing 40+ companies on Piazza Careers\n        \u003c/h2\u003e\n        \u003cdiv class=\"careers_intro_image_wrapper\"\u003e\n          \u003cdiv class=\"clearfix\"\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/square\" class=\"careers_intro_image square\"\u003e\u003c/a\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/spotify\" class=\"careers_intro_image spotify\"\u003e\u003c/a\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/spacex\" class=\"careers_intro_image spacex\"\u003e\u003c/a\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/microsoft\" class=\"careers_intro_image microsoft\"\u003e\u003c/a\u003e\n          \u003c/div\u003e\n\n          \u003cdiv class=\"clearfix\"\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/palantir\" class=\"careers_intro_image palantir\"\u003e\u003c/a\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/mongodb\" class=\"careers_intro_image mongodb\"\u003e\u003c/a\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/quora\" class=\"careers_intro_image quora\"\u003e\u003c/a\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/box\" class=\"careers_intro_image box\"\u003e\u003c/a\u003e\n          \u003c/div\u003e\n\n          \u003cdiv class=\"clearfix\"\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/airbnb\" class=\"careers_intro_image airbnb\"\u003e\u003c/a\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/pinterest\" class=\"careers_intro_image pinterest\"\u003e\u003c/a\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/yelp\" class=\"careers_intro_image yelp\"\u003e\u003c/a\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/evernote\" class=\"careers_intro_image evernote\"\u003e\u003c/a\u003e\n          \u003c/div\u003e\n\n          \u003cdiv class=\"clearfix\"\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/espn\" class=\"careers_intro_image espn\"\u003e\u003c/a\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/flipboard\" class=\"careers_intro_image flipboard\"\u003e\u003c/a\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/jane_street\" class=\"careers_intro_image jane_street\"\u003e\u003c/a\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/fitbit\" class=\"careers_intro_image fitbit\"\u003e\u003c/a\u003e\n          \u003c/div\u003e\n\n          \u003cdiv class=\"clearfix\"\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/lyft\" class=\"careers_intro_image lyft\"\u003e\u003c/a\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/opower\" class=\"careers_intro_image opower\"\u003e\u003c/a\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/spire\" class=\"careers_intro_image spire\"\u003e\u003c/a\u003e\n            \u003ca href=\"/careers/dashboard#/company_profile/affirm\" class=\"careers_intro_image affirm\"\u003e\u003c/a\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        \u003cp class=\"text-center\"\u003e\n          \u003cbutton class=\"btn btn-success close_careers_intro_browse\"\u003e\n            Browse company profiles\n          \u003c/button\u003e\n        \u003c/p\u003e\n        \u003cdiv class=\"text-center explanation\"\u003e\n          * We do not expose your contact information nor your Piazza Q\u0026amp;A discussions.\n        \u003c/div\u003e\n      \u003c/li\u003e\n      \u003cli\u003e\n        \u003ca href=\"/careers/dashboard#/companies\" class=\"careers_dashboard_link\" target=\"_blank\"\u003e\n          \u003c!--\n          Piazza Careers\n        --\u003e\n          Search Companies\n        \u003c/a\u003e\n      \u003c/li\u003e\n      \u003cli class=\"dropdown careers_dropdown careers_icons\"\u003e\n          \u003ca data-toggle=\"dropdown\" href=\"#\" class=\"has_icon\" target=\"_blank\" onclick=\"P.top_bar.displayCareersNotifications(true)\"\u003e\n            \u003cspan class=\"career_notification careers_notifications_count\" style=\"display: none;\"\u003e\u003c/span\u003e\n            \u003c!--\u003ci class=\"glyphicon glyphicon-folder-open icon-folder-open icon-white\" style = \"color: white;\"\u003e\n            \u003c/i\u003e--\u003e\n            \u003cdiv class=\"globe_def\" \u003e\n              \u003cimg src=\"/images/splash/layout/topbar/globe_icon.png\" \u003e\u003c/img\u003e\n            \u003c/div\u003e\n          \u003c/a\u003e\n          \u003cdiv class=\"dropdown-menu note_dropdown\"\u003e\n          \u003cul class=\"careers_notifications_wrapper careers_dropdown_notify\" role=\"menu\" aria-labelledby=\"dropdownMenu\"\u003e\n            \u003cli class=\"careers_notification clearfix\"\u003e\n              \u003cdiv class=\"careers_notification_content\"\u003e\n                \u003cdiv class=\"notification_title\"\u003e\n                  No Notifications Yet...\n                \u003c/div\u003e\n              \u003c/div\u003e\n            \u003c/li\u003e\n\n        \u003c!-- show this for student search notification // replace # classmates and class number --\u003e\n            \u003cli class=\"careers_notification clearfix\"\u003e\n              \u003cdiv class=\"careers_notification_content student_search_notification\"\u003e\n                \u003cdiv class=\"notification_title\"\u003e\n                  22 of your classmates in CS229 have listed where they've worked. \u003ca href=\"\"\u003eSee where\u003c/a\u003e\n                \u003c/div\u003e\n              \u003c/div\u003e\n            \u003c/li\u003e\n          \u003c/ul\u003e\n          \u003c!--\n          \u003cdiv class = \"control_wrap\"\u003e\u003ca tabindex=\"-1\" href=\"#Control_Note\" data-toggle=\"modal\" class=\"controlling_notes centering_notes color_change\" onclick=\"P.top_bar.displayCareersNotificationsOptions('top')\"\u003eControl Notifications\u003c/a\u003e\u003c/div\u003e\n        --\u003e\n        \u003c/div\u003e\n      \n      \u003c/li\u003e\n      \u003cli class=\"dropdown careers_dropdown careers_icons\"\u003e\n        \u003ca id=\"events_icon\" href=\"/careers/dashboard#/events\" class=\"has_icon topbar_events_icon\" target=\"_blank\"\u003e\n          \u003cspan class=\"career_notification event_count\" style=\"display: none;\"\u003e\u003c/span\u003e\n          \u003ci class=\"glyphicon glyphicon-calendar icon-calendar icon-white\"\u003e\n          \u003c/i\u003e\n        \u003c/a\u003e\n        \u003cul class=\"dropdown-menu topbar_dropdown_menu events_dropdown_menu\" role=\"menu\"\u003e\n          \u003cli class=\"dropdown_header\"\u003e\n            \u003c!--\n            \u003ch3 id=\"rsvp_header_1\"\u003e\n              Click an event to RSVP\n            \u003c/h3\u003e\n          --\u003e\n            \u003ch4 id=\"rsvp_header_2\" style=\"display:none;\"\u003e\n              \u003cspan class=\"triangle\"\u003e!\u003c/span\u003e\n              \u003cspan class=\"rsvp_message\"\u003eYou've RSPVed!\u003c/span\u003e\n              \u003cspan id=\"rsvp_company_list\"\u003e\u003c/span\u003e \u003cspan\u003ewill be reviewing your profile. \u003c/span\u003e\n              Your profile is \u003cspan id=\"profile_progress\"\u003e\u003c/span\u003e% complete.\n              \u003cspan id=\"rsvp_company_have_resume\"\u003e\u003ca href=\"/careers/dashboard#/my_profile\" target=\"_blank\"\u003eUpdate your profile\u003c/a\u003e\u003c/span\u003e\n            \u003c/h4\u003e\n            \u003cbutton type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-hidden=\"true\"\u003e×\u003c/button\u003e\n          \u003c/li\u003e\n          \u003cdiv id=\"event_cal_popover\"\u003e\u003c/div\u003e\n          \u003cli class=\"dropdown_footer clearfix\"\u003e\n            \u003cdiv class=\"event_lengend_wrapper pull-right clearfix\"\u003e\n              \u003cdiv class=\"event_legend clearfix\"\u003e\n                \u003cspan class=\"event_legend_color rsvp\"\u003e\u003c/span\u003e\n                \u003cspan class=\"event_legend_label\"\u003e\n                  RSVP'ed\n                \u003c/span\u003e\n              \u003c/div\u003e\n              \u003cdiv class=\"event_legend clearfix\"\u003e\n                \u003cspan class=\"event_legend_color new\"\u003e\u003c/span\u003e\n                \u003cspan class=\"event_legend_label\"\u003e\n                  New Event\n                \u003c/span\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n            \u003cdiv class=\"pull-left\"\u003e\n              \u003ca href=\"/careers/dashboard#/events\" target=\"_blank\"\u003eSee All Events \u0026#187;\u003c/a\u003e\n            \u003c/div\u003e\n          \u003c/li\u003e\n        \u003c/ul\u003e\n      \u003c/li\u003e\n      \u003cli class=\"careers_icons\"\u003e\n        \u003ca href=\"/careers/dashboard#/messages/reload_0\" class=\"has_icon\" id=\"messages_icon\" target=\"_blank\"\u003e\n          \u003cspan id=\"notifications_text\" class=\"career_notification message_count\" style=\"display: none;\"\u003e\u003c/span\u003e\n          \u003ci class=\"glyphicon glyphicon-comment icon-comment icon-white\"\u003e\n          \u003c/i\u003e\n        \u003c/a\u003e\n      \u003c/li\u003e\n    \u003c/ul\u003e\n  \u003c/div\u003e\n\n  \u003c!-- Green bar update summer job info --\u003e\n  \u003cdiv class=\"green_message_bar message_bar_summer clearfix\" id=\"headed_to_banner\" style=\"display: none;\"\u003e\n    \u003cdiv class=\"summer_info_block clearfix\"\u003e\n      \u003cdiv class=\"pull-left clearfix\"\u003e\n        \u003cdiv class=\"summer_message_title\"\u003e\n          Update where you’re headed for the summer so that employers can invite you to local events in their area.\n        \u003c/div\u003e\n        \u003cdiv class=\"summer_info_selectors\"\u003e\n          \u003cinput id=\"company_banner\" type=\"text\" placeholder=\"Company\"\u003e\n          \u003cinput id=\"role_banner\" type=\"text\" placeholder=\"Role\"\u003e\n          \u003cinput id=\"location_banner\" type=\"text\" placeholder=\"Location\"\u003e\n          \u003cinput id=\"summer_email_banner\" type=\"text\" placeholder=\"Summer email\"\u003e\n          \u003ca class=\"save_summer_info_button btn btn-default btn-small\" onclick=\"P.top_bar.updateHeadedToBanner()\"\u003eSave\u003c/a\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"hide_link\"\u003e\n        \u003ca href=\"#\"\u003e\n          \u003ci class=\"icon-remove icon-white\" onclick=\"P.top_bar.hideHeadedToBanner()\"\u003e\u003c/i\u003e\n        \u003c/a\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n    \u003c!--Appears when user saves email--\u003e\n    \u003cdiv class=\"third_message clearfix\" style=\"display: none;\"\u003e\n      \u003cp class=\"successful_save\"\u003eSaved! Please check the email you just added for a confirmation link. \u003c/p\u003e\n    \u003c/div\u003e\n\n  \u003c/div\u003e\n\n\n\u003c/div\u003e\n\u003cdiv class=\"modal_background\" style=\"display: none;\" id=\"waitlist_modal_with_image\"\u003e\n  \u003cdiv class=\"waitlist_modal clearfix\"\u003e\n    \u003ch1\u003eJoin the Academic Network of 1M+ students on Piazza\u003c/h1\u003e\n    \u003cdiv class=\"first_view\"  id=\"image_first_view\"\u003e\n      \u003cdiv class=\"selectors text-center\"\u003e\n        \u003cbutton class=\"sign_up\" onclick=\"P.top_bar.sign_up_waitlist()\"\u003eSign up for the waitlist\u003c/button\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"second_view\" style=\"display: none;\" id=\"image_second_view\"\u003e\n      \u003ch4\u003eWe've added you to the waitlist and will notify you when the tools are ready.\u003c/h4\u003e\n      \u003cdiv class=\"selectors text-center\"\u003e\n        \u003cbutton class=\"sign_up\" onclick=\"P.top_bar.exit_waitlist()\"\u003eClose\u003c/button\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"image_wrapper clearfix\"\u003e\n      \u003cdiv class=\"half_section\"\u003e\n        \u003cdiv class=\"record_image\"\u003e\n          \u003cdiv class=\"image_name\"\u003e\u003c/div\u003e\n        \u003c/div\u003e\n        \u003ch4 class=\"stat qa\"\u003eBuild upon your Piazza Q\u0026amp;A\u003cbr\u003eactivity\u003c/h4\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"half_section\"\u003e\n        \u003cdiv class=\"network_image\"\u003e\n          \u003cdiv class=\"image_name\"\u003e\u003c/div\u003e\n        \u003c/div\u003e\n        \u003ch4 class=\"stat data_p\"\u003eBuild upon your Q\u0026amp;A\u003cbr\u003eclassmates\u003c/h4\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"clearfix\"\u003e\n      \u003ca href=\"#\" class=\"not_for_me pull-right\" onclick=\"P.top_bar.reject_waitlist()\" id=\"image_dismiss\"\u003eDismiss\u003c/a\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n\n\u003cdiv class=\"modal_background\" style=\"display: none;\" id=\"waitlist_modal_without_image\"\u003e\n  \u003cdiv class=\"waitlist_modal clearfix\"\u003e\n    \u003ch1\u003eJoin the Academic Network of 1M+ students on Piazza\u003c/h1\u003e\n    \u003cdiv class=\"first_view\" id=\"no_image_first_view\"\u003e\n      \u003cdiv class=\"checkbox_options\"\u003e\n        \u003clabel class=\"checkbox\"\u003e\n          \u003cinput type=\"checkbox\" value=\"\" id=\"activity_checkbox\" checked\u003e\n          Seed with my class activity on Piazza \n        \u003c/label\u003e\n        \u003clabel class=\"checkbox\"\u003e\n          \u003cinput type=\"checkbox\" value=\"\" id=\"classmates_checkbox\" checked\u003e\n          Seed with my classmates on Piazza\n        \u003c/label\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"selectors text-center\"\u003e\n        \u003cbutton class=\"sign_up\" onclick=\"P.top_bar.sign_up_waitlist()\"\u003eSign up for the waitlist\u003c/button\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"second_view\" style=\"display: none;\" id=\"no_image_second_view\"\u003e\n      \u003ch4\u003eWe've added you to the waitlist and will notify you when the tools are ready.\u003c/h4\u003e\n      \u003cdiv class=\"selectors text-center\"\u003e\n        \u003cbutton class=\"sign_up\" onclick=\"P.top_bar.exit_waitlist()\"\u003eClose\u003c/button\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"clearfix\"\u003e\n      \u003ca href=\"#\" class=\"not_for_me pull-right\" onclick=\"P.top_bar.reject_waitlist()\" id=\"no_image_dismiss\"\u003eDismiss\u003c/a\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n\n\u003c!-- Modal --\u003e\n\u003c!-- \u003cdiv id=\"graduation_email_modal\" class=\"modal hide fade\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"myModalLabel\" aria-hidden=\"true\"\u003e\n  \u003cbutton type=\"button\" class=\"close graduation_esc_btn\" data-dismiss=\"modal\" aria-hidden=\"true\"\u003e×\u003c/button\u003e\n  \u003cdiv class=\"graduation_modal_container\"\u003e\n    \u003ch3 class=\"header_text\"\u003eYou're graduating.\u003c/h3\u003e\n    \u003cdiv class=\"subtext\"\u003eKeep access to your classes and your network on Piazza.\n    Classmates and employers want to keep in touch.\u003c/div\u003e\n    \u003cdiv class=\"email_box\"\u003e\n      \u003cdiv class=\"profile_circle\"\u003e\u003c/div\u003e\n      \u003cdiv class=\"name\"\u003eDavid Tilley\u003c/div\u003e\n      \u003cdiv class=\"current_email\"\u003edavid@david.com\u003c/div\u003e\n      \u003cinput type=\"text\" class=\"permanent_email_input\" placeholder=\"Permanent email\"\u003e\n      \u003cbutton class=\"submit_button\"\u003eSubmit\u003c/button\u003e\n      \u003ca href=\"\" class=\"not_graduating_link\"\u003eI'm not graduating soon\u003c/a\u003e\n    \u003c/div\u003e\n    \u003ca href=\"\" class=\"later_link\"\u003eI'll do this later\u003c/a\u003e\n  \u003c/div\u003e\n\u003c/div\u003e --\u003e\n");</script>

<script type="text/javascript">
P.modules.loadModule("feed", {}, null, "      \u003cdiv id=\"feed\" class=\"page_feed\"\u003e\n\n        \u003c!-- Where popups go for feed: --\u003e\n        \u003c!--  \u003cul class=\"careers_notifications_wrapper careers_feed_notify popup_nof_wrapper\" style=\"display:none;\"\u003e\u003c/ul\u003e --\u003e\n\n        \u003cdiv class=\"dashboard_toolbar page_feed_tool_bar\"\u003e\n          \u003cdiv class=\"hide_show_feed hide_feed\" tutorialw=\"Collapse feed\" onclick=\"PEM.fire('showHideFeed', false)\"\u003e\u0026#x25C4;\u003c/div\u003e\n          \u003cul id=\"page_feed_filter_buttons\"\u003e\n            \u003cli id=\"shortcut_unread\" onclick=\"PEM.fire('filterFeed', {filter:'unread'})\" {{#if user.config.feed.unread}}style=\"display:list-item;\"{{/if}}\u003eUnread\u003c/li\u003e\n            \u003cli id=\"shortcut_updated\" onclick=\"PEM.fire('filterFeed', {filter:'updated'})\" {{#if user.config.feed.updated}}style=\"display:list-item;\"{{/if}}\u003eUpdated\u003c/li\u003e\n            \u003cli id=\"shortcut_unresolved\" onclick=\"PEM.fire('filterFeed', {filter:'unresolved'})\" {{#if user.config.feed.unresolved}}style=\"display:list-item;\"{{/if}}\u003eUnresolved\u003c/li\u003e\n            \u003cli id=\"shortcut_due\" onclick=\"PEM.fire('filterFeed', {filter:'due'})\" {{#if user.config.feed.due}}style=\"display:list-item;\"{{/if}}\u003eDue\u003c/li\u003e\n            \u003cli id=\"shortcut_following\" onclick=\"PEM.fire('filterFeed', {filter:'following'})\" {{#if user.config.feed.following}}style=\"display:list-item;\"{{/if}}\u003eFollowing\u003c/li\u003e\n            \u003cli id=\"shortcut_hidden\" onclick=\"PEM.fire('filterFeed', {filter:'hidden'})\" {{#if user.config.feed.hidden}}style=\"display:list-item;\"{{/if}}\u003eArchived\u003c/li\u003e\n            \u003cli id=\"shortcut_hide_group_posts\" onclick=\"PEM.fire('filterFeed', {filter:'hide_group_posts'})\" {{#if user.config.feed.hide_group_posts}}style=\"display:list-item;\"{{/if}}\u003eHide Group Posts\u003c/li\u003e\n          \u003c/ul\u003e\n          \u003cspan class=\"feed_options_arrow\"\u003e▾\u003c/span\u003e\n          \u003cdiv class=\"page_feed_options\"\u003e\u003c/div\u003e\n          \u003cul id=\"page_feed_options_dropdown\" class=\"general_dropdown\"\u003e\n            \u003cli class=\"manual_filter filter_show_all active\" filter=\"\"\u003e\u003ci class=\"icon-ok opt_check\"\u003e\u003c/i\u003eAll\u003c/li\u003e\n            \u003cli class=\"manual_filter filter_activity\" filter=\"unread\"\u003e\u003ci class=\"icon-ok opt_check\"\u003e\u003c/i\u003eUnread\u003c/li\u003e\n            \u003cli class=\"manual_filter filter_updated\" filter=\"updated\"\u003e\u003ci class=\"icon-ok opt_check\"\u003e\u003c/i\u003eUpdated\u003c/li\u003e\n            \u003cli class=\"manual_filter filter_unanswered\" filter=\"unresolved\"\u003e\u003ci class=\"icon-ok opt_check\"\u003e\u003c/i\u003eUnresolved\u003c/li\u003e\n            \u003cli class=\"manual_filter filter_due\" filter=\"due\"\u003e\u003ci class=\"icon-ok opt_check\"\u003e\u003c/i\u003eDue for answer\u003c/li\u003e\n            \u003cli class=\"manual_filter filter_following\" filter=\"following\"\u003e\u003ci class=\"icon-ok opt_check\"\u003e\u003c/i\u003eFollowing\u003c/li\u003e\n            \u003cli class=\"manual_filter filter_hidden\" filter=\"hidden\"\u003e\u003ci class=\"icon-ok opt_check\"\u003e\u003c/i\u003eArchived\u003c/li\u003e\n            \u003cli class=\"manual_filter filter_hidden section_end\" filter=\"instructors\"\u003e\u003ci class=\"icon-ok opt_check\"\u003e\u003c/i\u003eInstructor Posts\u003c/li\u003e\n            \u003cli class=\"manual_filter filter_hide_group_posts section_end\" filter=\"hide_group_posts\"\u003e\u003ci class=\"icon-ok opt_check\"\u003e\u003c/i\u003eHide Group Posts\u003c/li\u003e\n            \u003cli style=\"display:none;\" filter=\"updated\" class=\"open_sort_option\" tutorialw=\"Sort feed by date\" \u003e\u003ci class=\"icon-ok opt_check\"\u003e\u003c/i\u003eSort by Date\u003c/li\u003e\n            \u003cli style=\"display:none;\" filter=\"score\" class=\"open_sort_option\" tutorialw=\"Sort feed by views\" \u003e\u003ci class=\"icon-ok opt_check\"\u003e\u003c/i\u003eSort by Views\u003c/li\u003e\n            \u003cli style=\"display:none;\" filter=\"like\" class=\"open_sort_option section_end\" tutorialw=\"Sort feed by likes\" \u003e\u003ci class=\"icon-ok opt_check\"\u003e\u003c/i\u003eSort by Likes\u003c/li\u003e\n            \u003cli id=\"markAllRead\" class=\"section_end\" tutorialw=\"Mark all posts as read\" \u003e\u003ci class=\"icon-ok opt_check\"\u003e\u003c/i\u003eMark All Read\u003c/li\u003e\n            \u003cli class=\"show_feed_details active\" tutorialw=\"Hide post descriptions from your feed\"\u003e\u003ci class=\"icon-ok opt_check\"\u003e\u003c/i\u003eView in Compact Mode\u003c/li\u003e\n            \u003cli class=\"show_icons_in_feed {{#if user.config.feed.icons}}active{{/if}}\" tutorialw=\"Identify which posts have instructors' or students' answers\"\u003e\u003ci class=\"icon-ok opt_check\"\u003e\u003c/i\u003eShow Helper Icons\u003c/li\u003e\n          \u003c/ul\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"share_classes_message_box blue\" id=\"share_classes_message_box\" style=\"display:none;\"\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"share_classes_message_box\" id=\"enter_grad_year_major\" style=\"display:none;\"\u003e\n          \u003ch2\u003eWhat year are you graduating?\u003c/h2\u003e\n          \u003cdiv class=\"clearfix content_section\"\u003e\n            \u003cdiv class=\"clearfix\"\u003e\n              \u003clabel class=\"radio inline\"\u003e\n                \u003cinput type=\"radio\" id=\"2013\" name=\"grad_year\" value=\"2013\"\u003e 2013\n              \u003c/label\u003e\n              \u003clabel class=\"radio inline\"\u003e\n                \u003cinput type=\"radio\" id=\"2014\" name=\"grad_year\" value=\"2014\"\u003e 2014\n              \u003c/label\u003e\n              \u003clabel class=\"radio inline\"\u003e\n                \u003cinput type=\"radio\" id=\"2015\" name=\"grad_year\" value=\"2015\"\u003e 2015\n              \u003c/label\u003e\n              \u003clabel class=\"radio inline\"\u003e\n                \u003cinput type=\"radio\" id=\"2016\" name=\"grad_year\" value=\"2016\"\u003e 2016\n              \u003c/label\u003e\n              \u003clabel class=\"radio inline\"\u003e\n                \u003cinput type=\"radio\" id=\"2017\" name=\"grad_year\" value=\"2017\"\u003e 2017\n              \u003c/label\u003e\n            \u003c/div\u003e\n            \u003cdiv class=\"clearfix\"\u003e\n              \u003clabel class=\"radio inline\"\u003e\n                \u003cinput type=\"radio\" id=\"2017\" name=\"grad_year\" value=\"Other\"\u003e Other\n              \u003c/label\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"share_classes_message_box\" id=\"submit_resume\" style=\"display:none;\"\u003e\n          \u003cspan class=\"message_box_close_button\" id=\"close_resume_feed_box\"\u003e\u003ci class=\"icon-remove icon-white\"\u003e\u003c/i\u003e\u003c/span\u003e\n          \u003cdiv class=\"first_view clearfix\" style=\"display: block;\"\u003e\n            \u003ch2\u003eConnect with other brilliant students on Piazza\u003c/h2\u003e\n            \u003cdiv class=\"content_section\"\u003e\n              \u003cdiv class=\"btn btn-large\" id=\"upload_resume_feed_btn\"\u003eUpload your resume\u003c/div\u003e\n\n\n             \u003cdiv class=\"center_button\"\u003e We'll connect you with students like yourself.\u003c/div\u003e\n            \u003c/div\u003e\n            \u003cform method=\"POST\" id=\"upload_form_feed\" class=\"hide\" action=\"/careers/upload_resume\" enctype=\"multipart/form-data\" target=\"hidden-upload-frame-feed\"\u003e\n              \u003cinput type=\"file\" id=\"upload_form_file\" name=\"file\" onchange=\"P.feed.startUpload();\"/\u003e\n            \u003c/form\u003e\n            \u003ciframe id=\"hidden-upload-frame-feed\" name=\"hidden-upload-frame-feed\" style=\"display: none\" onload=\"P.feed.onUploadComplete();\"\u003e\u003c/iframe\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"second_view clearfix\" style=\"display: none;\"\u003e\n            \u003ch2\u003eUpload successful\u003c/h2\u003e\n            \u003ch4\u003eYou'll see students similar to yourself starting Spring 2014. Till then, companies are also starting to discover students on Piazza.\u003c/h4\u003e\n            \u003cdiv class=\"clearfix content_section\"\u003e\n              \u003clabel class=\"checkbox\"\u003e\n                \u003cinput type=\"checkbox\" value=\"\" checked id=\"feed_resume_checkbox\"\u003e\n                I'd like companies to be able to view my resume.\n              \u003c/label\u003e\n              \u003cp class=\"center_button\"\u003e\n                \u003cbutton class=\"btn btn-small\" id=\"okay_resume_feed_box\"\u003eOK\u003c/button\u003e\n              \u003c/p\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        \u003c!--request invite to join Piazza for professionals network. This is to collect data on who is interested.--\u003e\n        \u003cdiv class=\"share_classes_message_box\" id=\"request_invite_to_network\" style=\"display: none;\"\u003e\n          \u003cdiv class=\"first_view clearfix\"\u003e\n            \u003ch2\u003eDon’t lose the academic differentiation you’re building up on Piazza.\u003c/h2\u003e\n            \u003cp\u003eComing January 2016, tools to build upon your class activity and network to take into your career.\u003c/p\u003e\n            \u003cdiv class=\"content_section text-center\"\u003e\n              \u003cbutton class=\"btn btn-success\" id=\"invite_waitlist\"\u003eRequest Invite\u003c/button\u003e\n            \u003c/div\u003e\n            \u003ch2 id=\"confirmation_waitlist\" style=\"display: none;\"\u003eRequest sent. Stay tuned.\u003c/h2\u003e\n            \u003cdiv class=\"clearfix\"\u003e\n              \u003ca href=\"#\" class=\"pull-right\" id=\"dismiss_waitlist\"\u003eDismiss\u003c/a\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"second_view clearfix\" style=\"display: none;\"\u003e\n            \u003ch2\u003eRequest successful\u003c/h2\u003e\n            \u003cp\u003eWe've added you to the waitlist and will notify you when the tools are ready.\u003c/p\u003e\n            \u003cdiv class=\"clearfix content_section\"\u003e\n              \u003cp class=\"center_button text-center\"\u003e\n                \u003cbutton class=\"btn\"\u003eOK\u003c/button\u003e\n              \u003c/p\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"page_feed_search_wrapper clearfix\"\u003e\n          \u003cdiv id=\"feed_search\"\u003e\n            \u003cbutton id=\"new_post_button\" class=\"btn-primary right btn-small\" onclick=\"dataLayer.push({'event': 'eventTracker', 'eventCat': 'Post', 'eventAct': 'New Post Button', 'eventLbl': '', 'eventVal': 0}); \"\u003e\u003ci class=\"icon-file icon-white\"\u003e\u003c/i\u003e New Post\u003c/button\u003e\n            \u003cdiv class=\"search_bar\"\u003e\n              \u003cdiv class=\"clearButton\" id=\"clear-search-button\"\u003e\u003c/div\u003e\n              \u003cdiv class=\"icon\"\u003e\u003c/div\u003e\n              \u003cinput type=\"text\" id=\"search-box\" placeholder=\"Search or add a post...\"/\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"search_tool_tip\"\u003e\u003c/div\u003e\n        \u003cdiv id=\"feed_search_disabled\" class=\"alert search_tool_tip grey_tool_tip\"\u003e\n          \u003cbutton id=\"dismiss_feed_search_disabled\" type=\"button\" class=\"close\"\u003e\u0026times;\u003c/button\u003e\n          Search is disabled for demo classes to preserve privacy.\n        \u003c/div\u003e\n        \u003cdiv class=\"page_feed_filter_feed\" id=\"filter_message\"\u003e\n          \u003cspan\u003eFiltering by:\u003c/span\u003e \u003cstrong id=\"filter_by\"\u003eUnread Posts\u003c/strong\u003e \u003ca class=\"bookmark_filter_link\" href=\"#\" onclick=\"PEM.fire('toggleFeedShortcut');return false;\"\u003eRemove shortcut\u003c/a\u003e\n          \u003cdiv class=\"clear_filter\" onclick=\"PEM.fire('filterFeed');return false;\" style=\"display:inline-block;cursor:pointer;\"\u003e\u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"page_feed_filter_feed\" id=\"filter_sort_method\" style=\"display:none\"\u003e\n          \u003cdiv class=\"search_tips_header\"\u003e\n            \u003ca href=\"#\" class=\"search_tips_toggle\"\u003eSearch Tips \u003cspan class=\"search_tips_arrow down\"\u003e\u003c/span\u003e\u003c/a\u003e\u003cspan class=\"initial_message\"\u003e\u0026#40;Learn how to increase the relevance of your search results!\u0026#41;\u003c/span\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"search_tips\"\u003e\n            \u003ctable\u003e\n              \u003ctr\u003e\n                \u003cth\u003e\n                  If you type:\n                \u003c/th\u003e\n                \u003cth class=\"results\"\u003e\n                  You'll see:\n                \u003c/th\u003e\n              \u003c/tr\u003e\n              \u003ctr\u003e\n                \u003ctd\u003e\n                  Office hours\n                \u003c/td\u003e\n                \u003ctd\u003e\n                  only results that contain office \u003cstrong\u003eand\u003c/strong\u003e hours\n                \u003c/td\u003e\n              \u003c/tr\u003e\n              \u003ctr\u003e\n                \u003ctd\u003e\n                  Office |hours\n                \u003c/td\u003e\n                \u003ctd\u003e\n                  results that contain office \u003cstrong\u003eor\u003c/strong\u003e hours\n                \u003c/td\u003e\n              \u003c/tr\u003e\n              \u003ctr\u003e\n                \u003ctd\u003e\n                  Office -hours\n                \u003c/td\u003e\n                \u003ctd\u003e\n                  only results that contain office \u003cstrong\u003ebut not\u003c/strong\u003e hours\n                \u003c/td\u003e\n              \u003c/tr\u003e\n              \u003ctr\u003e\n                \u003ctd\u003e\n                  \u0026quot;Office Hours\u0026quot;\n                \u003c/td\u003e\n                \u003ctd\u003e\n                  only results with the \u003cstrong\u003eexact phrase\u003c/strong\u003e \u0026quot;office hours\u0026quot;\n                \u003c/td\u003e\n              \u003c/tr\u003e\n              \u003ctr\u003e\n                \u003ctd\u003e\n                  subject:\"Office hours\"\n                \u003c/td\u003e\n                \u003ctd\u003e\n                  posts with the \u003cstrong\u003etitle\u003c/strong\u003e \u0026quot;Office hours\u0026quot;\n                \u003c/td\u003e\n              \u003c/tr\u003e\n              \u003ctr\u003e\n                \u003ctd\u003e\n                  contributor:\"Mathew Ewer\"\n                \u003c/td\u003e\n                \u003ctd\u003e\n                  posts with \u003cstrong\u003econtributions by\u003c/strong\u003e Mathew Ewer\n                \u003c/td\u003e\n              \u003c/tr\u003e\n              \u003ctr\u003e\n                \u003ctd\u003e\n                  folder:hw1\n                \u003c/td\u003e\n                \u003ctd\u003e\n                  posts \u003cstrong\u003ewithin the folder\u003c/strong\u003e \u0026quot;hw1\u0026quot;\n                \u003c/td\u003e\n              \u003c/tr\u003e\n            \u003c/table\u003e\n            \u003cspan\u003eSort results by: \u003c/span\u003e\u003cdiv class=\"sort_results_by\" id=\"sort_by_rel\"\u003eRelevance\u003c/div\u003e\u003cdiv class=\"sort_results_by active\" id=\"sort_by_date\"\u003eDate\u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv id=\"noresults\" style=\"display: block;\"\u003e\u003cp\u003eno results found\u003c/p\u003e\u003c/div\u003e\n        \u003cdiv id=\"feed_expanded\" class=\"alert search_tool_tip grey_tool_tip\"\u003e\n          \u003cbutton id=\"dismiss_feed_expanded_msg\" type=\"button\" class=\"close\" data-dismiss=\"alert\"\u003e\u0026times;\u003c/button\u003e\n          Your feed view has been changed from compact view to expanded view. Click on the gear icon to edit this setting.\n        \u003c/div\u003e\n        \u003cdiv id=\"question_feed_questions\" class=\"scrollable\"\u003e\u003c/div\u003e\n  \t\t\t\u003cdiv id=\"split_bar\"\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"invite_members\" style=\"display:none;\"\u003e\n          \u003cdiv class=\"invite_members_header default_text\"\u003eInvite Members \u003ca href=\"#\" class=\"invite_members_dismiss_button\"\u003eDismiss\u003c/a\u003e\u003c/div\u003e\n          \u003cdiv class=\"invite_members_header confirmation_text\" style=\"display:none;\"\u003eInvites Sent!\u003c/div\u003e\n          \u003ctextarea class=\"invite_members_input\" rows=\"2\" placeholder=\"Enter email(s) and press Enter key to send\"\u003e\u003c/textarea\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n\n");</script>

