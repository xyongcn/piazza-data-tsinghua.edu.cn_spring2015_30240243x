<script type="text/javascript">
P.modules.loadModule("user_careers_profile", {}, null, "\u003cdiv id=\"careers_profile\" class=\"{{#if forStudent}}student_view{{/if}}{{#if forCompany}}company_view{{/if}}\" uid=\"{{profile.user_id}}\" style=\"display:block;\"\u003e\n  {{#if canAdmin}}\n  \u003cdiv class=\"profile_body profile_nav clearfix\"\u003e\n    {{#if fromSearch}}\n    \u003ca href=\"#/search\" class=\"show_for_company action_link back_to_search\"\u003eBack to Search\u003c/a\u003e\n    {{/if}}\n    \u003cdiv class=\"profile_container_full\"\u003e\n      \u003ca href=\"#\" class=\"profile_nav_btn profile_nav_btn_previous\"\u003e\u003cspan class=\"circle_wrap left\"\u003e\u003ci class=\"icon-chevron-left\"\u003e\u003c/i\u003e\u003c/span\u003ePrevious Student\u003c/a\u003e\n      \u003cdiv class=\"company_status\"\u003e\n        Status:\n        \u003cselect class=\"status_value\" uid=\"{{profile.user_id}}\"\u003e\n          \u003coption value=\"-\" checked\u003e-\u003c/option\u003e\n          \u003coption value=\"Contacted\"\u003eContacted\u003c/option\u003e\n          \u003coption value=\"Interview Scheduled\"\u003eInterview Scheduled\u003c/option\u003e\n          \u003coption value=\"Interviewing\"\u003eInterviewing\u003c/option\u003e\n          \u003coption value=\"Phone Screened\"\u003ePhone Screened\u003c/option\u003e\n          \u003coption value=\"Offer Made\"\u003eOffer Made\u003c/option\u003e\n          \u003coption value=\"Hired\"\u003eHired\u003c/option\u003e\n          \u003coption value=\"Rejected\"\u003eRejected\u003c/option\u003e\n          \u003coption value=\"Other\"\u003eOther\u003c/option\u003e\n        \u003c/select\u003e\n      \u003c/div\u003e\n      \u003ca href=\"#\" class=\"profile_nav_btn profile_nav_btn_next\"\u003eNext Student\u003cspan class=\"circle_wrap right\"\u003e\u003ci class=\"icon-chevron-right\"\u003e\u003c/i\u003e\u003c/span\u003e\u003c/a\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  {{/if}}\n  {{#if user.showCareers}}\n  {{#if showPrivacy}}\n  \u003cdiv class=\"post_region_box_profile message profile_privacy_message\"\u003e\n    \u003cbutton type=\"button\" class=\"close close_privacy_msg\" data-dismiss=\"alert\"\u003e\u0026times;\u003c/button\u003e\n    This profile is \u003cstrong\u003enot visible to students or instructors\u003c/strong\u003e in your class and only visible to companies after you publish it.\n  \u003c/div\u003e\n  {{/if}}\n  {{/if}}\n  {{#if profile.onCompany}}\n\n\u003c!--   \u003cdiv class=\"clearfix careers_tagging\"\u003e\n    \u003cdiv class=\"clearfix career_tags_header\"\u003e\n      \u003cul class=\"careers_tag_list clearfix profile_company_tag_list\" {{#unless profile.company_tags}}style=\"display:none;\"{{/unless}}\u003e\n        \u003cli\u003e\u003cspan class=\"profile_label\"\u003eTags for this student:\u003c/span\u003e\u003c/li\u003e\n        \u003cdiv class=\"selected_tags\"\u003e\n        {{#each profile.company_tags}}\n        \u003cli\u003e\u003ca href=\"#\" class=\"tag folder selected\" onclick=\"return false;\"\u003e{{this}}\u003c/a\u003e\u003c/li\u003e\n        {{/each}}\n        \u003c/div\u003e\n      \u003c/ul\u003e\n      \u003ca href=\"#\" class=\"action_link show_tagging\" style=\"float:right;\"\u003eTag this student\u003c/a\u003e\n      \u003ca href=\"#\" class=\"action_link hide_tagging\" style=\"float:right;display:none\"\u003eDone tagging\u003c/a\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class=\"post_region_box_profile clearfix tagging_wrapper\" style=\"display:none;\"\u003e\n      \u003ch4\u003eTag This Student\u003c/h4\u003e\n      \u003cul class=\"careers_tag_list clearfix tag_list_profile\"\u003e\n        {{#each allTags}}\n        \u003cli tag=\"{{this}}\" class=\"li_tag_wrapper\"\u003e\u003ca href=\"#\" class=\"tag folder {{#ifContains ../profile.company_tags this}}selected{{/ifContains}}\" onclick=\"return P.user_careers_profile.toggleTag(this);\"\u003e{{this}}\u003c/a\u003e\u003c/li\u003e\n        {{/each}}\n      \u003c/ul\u003e\n      \u003cdiv class=\"clearfix create_tag_section\"\u003e\n        \u003cinput type=\"text\" class=\"new_tag_textbox\" placeholder=\"Enter tag name\"\u003e\n        \u003cbutton class=\"btn btn-small create_new_tag\"\u003eCreate New Tag\u003c/button\u003e\n      \u003c/div\u003e\n      \u003ca href=\"#manage_careers_tags\" role=\"button\" data-toggle=\"modal\" class=\"action_link\"\u003eManage Tags\u003c/a\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e --\u003e\n\n  \u003cdiv class=\"clearfix careers_tagging\"\u003e\n    \u003cul class=\"careers_tag_list tag_library clearfix\"\u003e\n      \u003clabel\u003eTag Library:\u003c/label\u003e\n      {{#each allTags}}\n      \u003cli\u003e\u003ca href=\"#\" class=\"careers_tag {{#ifContains ../profile.company_tags this}}active{{/ifContains}}\" onclick=\"return P.user_careers_profile.toggleTagFromLibrary(this);\"\u003e{{this}}\u003cspan class=\"remove_tag_wrap\"\u003e\u003ci class=\"icon-remove-sign remove_tag\" onclick=\"return P.user_careers_profile.deleteTag(this);\"\u003e\u003c/i\u003e\u003c/span\u003e\u003c/a\u003e\u003c/li\u003e\n      {{/each}}\n      \u003ca href=\"#\" class=\"manage_tag_toggle start_manage_tag_toggle\"\u003eManage Tag Library\u003c/a\u003e\n      \u003ca href=\"#\" class=\"manage_tag_toggle done_manage_tag_toggle\"\u003eDone Managing\u003c/a\u003e\n    \u003c/ul\u003e\n    \u003cul id=\"selected_user_tags\" class=\"careers_tag_list clearfix\"\u003e\n      {{#each profile.company_tags}}\n      \u003cli\u003e\u003ca href=\"#\" class=\"careers_tag active\" onclick=\"return P.user_careers_profile.toggleTag(this);\"\u003e{{this}}\u003c/a\u003e\u003c/li\u003e\n      {{/each}}\n      \u003cli id=\"add_tags_input\"\u003e\n        \u003cinput type=\"text\" id=\"add_tags\" placeholder=\"Click to Add Tags\"\u003e\n        \u003cul class=\"tags_autocomplete\" style=\"display:none;\"\u003e\n        \u003c/ul\u003e\n      \u003c/li\u003e\n    \u003c/ul\u003e\n\n    \u003ca href=\"#\" class=\"btn btn-mini show_tag_toggle tag_toggle\"\u003e\u003ci class=\"icon-chevron-up\"\u003e\u003c/i\u003e Show Tag Library\u003c/a\u003e\n    \u003ca href=\"#\" class=\"btn btn-mini hide_tag_toggle tag_toggle\"\u003e\u003ci class=\"icon-chevron-down\"\u003e\u003c/i\u003e Hide Tag Library\u003c/a\u003e\n  \u003c/div\u003e\n  {{/if}}\n  {{#if user.showCareers}}\n  {{#if profile.me}}\n  {{#unless profile.profile_settings.published}}\n  \u003cdiv class=\"post_region_box_profile message hide_for_student_preview show_for_student clearfix\"\u003e\n    \u003cdiv class=\"message_wrapper\"\u003e\n      \u003ch4 class=\"message_header\"\u003e\n        Are you ready to publish your profile?\n      \u003c/h4\u003e\n      \u003cp\u003ePublish your profile to allow companies to learn more about you.\u003c/p\u003e\n    \u003c/div\u003e\n    \u003cbutton class=\"btn btn-mini publish_button btn-primary publish_profile\"\u003ePublish Profile\u003c/button\u003e\n  \u003c/div\u003e\n  {{/unless}}\n  {{/if}}\n  {{/if}}\n  {{#if forStudent}}\n  \u003cdiv class='show_for_student_preview action_link stop_view_as_company'\u003e\u003ca href=\"#\" class=\"show_for_student action_link show_for_student_preview stop_view_as_company\"\u003eBack to student view of my profile\u003c/a\u003e\u003c/div\u003e\n  {{/if}}\n  {{#if user.showCareers}}\n  {{#if profile.me}}\n    \u003ca href=\"#\" class=\"action_link view_as_company\"\u003eSee company view of my profile\u003c/a\u003e\n  {{/if}}\n  {{/if}}\n  \u003cdiv class=\"post_region_box_profile header clearfix\"\u003e\n    \u003cdiv class=\"profile_header clearfix\"\u003e\n      {{#if profile.company_log}}\n         \u003cspan class=\"company_action_count\"\u003e\u003ca href=\"#company_actions_anchor\"\u003e{{profile.company_log.length}} Company Actions\u003c/a\u003e\u003c/span\u003e\n      {{/if}}\n      {{#if profile.me}}\n        \u003ca href=\"#\" class=\"show_for_student action_link edit_button hide_for_student_preview hide_for_edit edit_profile_header\"\u003eedit\u003c/a\u003e\n        \u003ca href=\"#\" class=\"show_for_student action_link save_button edit_profile_header show_for_edit\"\u003esave\u003c/a\u003e\n      {{/if}}\n      \u003cdiv class=\"profile_pic\"\u003e\n        {{#if profile.imagePath}}\n          \u003cimg id=\"thumb\" title=\"profile picture\" src=\"{{profile.imagePath}}\" onload=\"onImageLoad(event);\" style=\"width:100%; height: 100%;\"\u003e\n        {{else}}\n          \u003cimg id=\"thumb\" title=\"profile picture\" src=\"https://dvngeac8rg9mb.cloudfront.net/images/dashboard/common/default_user.png\" style=\"width:100%; height: 100%;\"\u003e\n        {{/if}}\n      \u003c/div\u003e\n      \u003cdiv class=\"profile_who\"\u003e\n        \u003ch2 class=\"profile_name\"\u003e{{profile.name}}\n        {{#if canAdmin}}\n        {{#if profile.requested_interview}}\n        \u003cspan class=\"label label-default\" style=\"background-color:#999999\";\u003eStudent Requested Interview\u003c/span\u003e\n        {{/if}}\n        {{/if}}\n        \u003c/h2\u003e\n        {{#if canAdmin}}\n        \u003cdiv class=\"fast_actions\"\u003e\n          \u003cdiv class=\"fast_actions_tips\" style=\"display:none;\"\u003e\n            \u003cdiv class=\"container\"\u003e\n              \u003ca href=\"#\" class=\"remove_btn remove_fast_actions_tips\"\u003eGot it \u003cspan class=\"icon-remove-circle\"\u003e\u003c/span\u003e\u003c/a\u003e\n              \u003ch3\u003e\n                We’ve added fast actions!\n                \u003cspan\u003eClick:\u003c/span\u003e\n              \u003c/h3\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n          \u003ci class=\"favorite_icon {{#if profile.my_favorite}}active{{/if}} favorite_link\" tipsy=\"Favorite\"\u003e\u003c/i\u003e\n          {{#ifContains profile.archived myid}}\n            \u003ci class=\"archive_icon archive active\" tipsy=\"Un-archive\"\u003e\u003c/i\u003e\n          {{else}}\n            \u003ci class=\"archive_icon archive\" tipsy=\"Archive\"\u003e\u003c/i\u003e\n          {{/ifContains}}\n          \u003ci class=\"send_message_icon send_message\" tipsy=\"Send a message\"\u003e\u003c/i\u003e\n          {{#if profile.has_correspondence}}\n          \u003ci class=\"see_message_icon see_messages active show_for_messages\" tipsy=\"View past messages\"\u003e\u003cspan class=\"see_message_count\"\u003e{{profile.has_correspondence}}\u003c/span\u003e\u003c/i\u003e\n          {{else}}\n          \u003ci class=\"see_message_icon\"\u003e\u003cspan class=\"see_message_count\"\u003e0\u003c/span\u003e\u003c/i\u003e\n          {{/if}}\n          {{#if profile.resume}}\n          \u003ca href=\"{{profile.resume}}\" uid=\"{{profile.user_id}}\" class=\"action_link download_resume\" target=\"_blank\"\u003e\u003ci class=\"resume_icon active\" tipsy=\"Download resume\"\u003e\u003c/i\u003e\u003c/a\u003e\n          {{else}}\n          \u003ci class=\"resume_icon\"\u003e\u003c/i\u003e\n          {{/if}}\n        \u003c/div\u003e\n        {{/if}}\n        \u003ch3 class=\"profile_school\"\u003e\u003cspan class=\"hide_for_edit\"\u003e{{profile.school}}\u003c/span\u003e\n          \u003cspan class=\"show_for_edit\"\u003e\n          \u003cselect id=\"new_school_id\" data-field=\"school_id\"\u003e\n            {{#each profile.schools}}\n            \u003coption value=\"{{this.id}}\" {{#ifEquals this.name ../profile.school}}selected{{/ifEquals}}\u003e{{this.name}}\u003c/option\u003e\n            {{/each}}\n          \u003c/select\u003e\n          \u003c/span\u003e\n        \u003c/h3\u003e\n\n        {{#if canAdmin}}\n        {{#if profile.visa}}\n        \u003cp class=\"visa_sponsorship hide_for_not_published\"\u003eVisa sponsorship\n          \u003cspan class=\"sponsorship_info hide_for_edit\"\u003e{{profile.visa}}\u003c/span\u003e\n        \u003c/p\u003e\n        {{/if}}\n        {{#ifEquals profile.status \"accepted_fulltime\"}}\n          \u003cspan class=\"label\" style=\"background-color:#f89406\";\u003eAccepted Fulltime Offer\u003c/span\u003e\n        {{/ifEquals}}\n        {{#ifEquals profile.status \"accepted_intern\"}}\n          \u003cspan class=\"label\" style=\"background-color:#f89406\";\u003eAccepted Internship Offer\u003c/span\u003e\n        {{/ifEquals}}\n        {{else}}\n        \u003cp class=\"visa_sponsorship hide_for_not_published\"\u003eVisa sponsorship\n          \u003cspan class=\"sponsorship_info hide_for_edit\"\u003e{{#if profile.visa}}{{profile.visa}}{{else}}\u003ci\u003eunknown\u003c/i\u003e{{/if}}\u003c/span\u003e\n          \u003cspan class=\"show_for_edit add_margin\"\u003e\n            \u003clabel class=\"radio inline\"\u003e\n              \u003cinput name=\"sponsorship_needed\" type=\"radio\" value=\"required\" data-field=\"visa\" {{#ifEquals profile.visa \"required\"}}checked{{/ifEquals}}\u003e required\n            \u003c/label\u003e\n            \u003clabel class=\"radio inline\"\u003e\n              \u003cinput name=\"sponsorship_needed\" type=\"radio\" value=\"not required\" data-field=\"visa\" {{#ifEquals profile.visa \"not required\"}}checked{{/ifEquals}}\u003e not required\n            \u003c/label\u003e\n          \u003c/span\u003e\n        \u003c/p\u003e\n        {{/if}}\n        \u003c!-- \u003cp class=\"marketing show_for_company\"\u003eViewed your company profile\u003c/p\u003e --\u003e\n\n\n      \u003c/div\u003e\n\n      {{#if profile.me}}\n      \u003cdiv class=\"profile_contact hide_for_not_published clearfix\"\u003e\n        \u003cdiv class=\"entry email clearfix\"\u003e\n          \u003clabel class=\"email_icon\"\u003e\u003c/label\u003e\n          \u003cp class=\"entry entry_value email hide_for_edit\"\u003e{{{profile.allEmails}}}\u003c/p\u003e\n          \u003cinput type=\"text\" class=\"profile_text_input show_for_edit\" value=\"{{profile.allEmails2}}\" data-field=\"contact.custom_email\"\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"entry phone clearfix\"\u003e\n          \u003clabel class=\"phone_icon\"\u003e\u003c/label\u003e\n          \u003cp class=\"entry entry_value phone hide_for_edit\"\u003e{{profile.contact.phone}}\u003c/p\u003e\n          \u003cinput type=\"text\" class=\"profile_text_input show_for_edit\" value=\"{{profile.contact.phone}}\" data-field=\"contact.phone\"\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"entry skype clearfix\"\u003e\n          \u003clabel class=\"skype_icon\"\u003e\u003c/label\u003e\n          \u003cp class=\"entry entry_value hide_for_edit skype\"\u003e{{profile.contact.skype}}\u003c/p\u003e\n          \u003cinput type=\"text\" class=\"profile_text_input show_for_edit\" value=\"{{profile.contact.skype}}\" data-field=\"contact.skype\"\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      {{/if}}\n    \u003c/div\u003e\n    \u003cdiv class=\"profile_actions clearfix\"\u003e\n      \u003cdiv class=\"profile_actions_wrapper show_for_company left_align\"\u003e\n        \u003c!-- \u003ca href=\"#\" class=\"action_link archive\"\u003eArchive\u003c/a\u003e\n        \u003cspan class=\"middot\"\u003e\u0026middot;\u003c/span\u003e --\u003e\n        {{#if canAdmin}}\n\u003c!--         \u003ca href=\"#\" class=\"action_link favorite\"\u003e{{#if profile.my_favorite}}Unfavorite{{/if}}{{#unless profile.my_favorite}}Favorite{{/unless}}\u003c/a\u003e\n        \u003cspan class=\"middot\"\u003e\u0026middot;\u003c/span\u003e\n        \u003ca href=\"#\" class=\"action_link send_message\"\u003eSend Message\u003c/a\u003e\n        \u003cspan class=\"middot show_for_messages\" {{#unless profile.has_correspondence}}style=\"display:none;\"{{/unless}}\u003e\u0026middot;\u003c/span\u003e\n        \u003ca href=\"#\" class=\"action_link see_messages show_for_messages\" {{#unless profile.has_correspondence}}style=\"display:none;\"{{/unless}}\u003e\n          {{#showCompanyUsers profile.message_arr}}See Messages{{/showCompanyUsers}}\n        \u003c/a\u003e --\u003e\n        \u003cdiv class=\"dropdown profile_actions_dropdown\"\u003e\n          \u003ca href=\"#\" class=\"dropdown-toggle\" dat