<script type="text/javascript">
P.modules.loadModule("clarifying_discussion", {}, null, "\u003cdiv id=\"clarifying_discussion\" class=\"post_region_box clarifying_discussion dashboard_element\"\u003e\n  \u003cdiv class=\"post_region_header clearFix\"\u003e\n    \u003cdiv class=\"post_title\"\u003efollowup discussions\u003c/div\u003e\n    \u003cdiv class=\"post_subtitle\"\u003efor lingering questions and comments\u003c/div\u003e\n  \u003c/div\u003e\n  {{#if content.config.resolved_followups}}\n  \u003cdiv class=\"post_region_header show_limited_posts clearFix\" id=\"resolved_followups\"\u003e\n    showing the last 30 followups:\n  \u003c/div\u003e\n  {{/if}}\n  \u003cdiv data-pats=\"followups\" class=\"post_region_content clarifying_discussion\"\u003e\n    {{#each content.followups}}\n      {{#followupMessage this}}{{/followupMessage}}\n    {{/each}}\n    \u003ch5 id=\"start_new_followup_header\"\u003eStart a new followup discussion\u003c/h5\u003e\n    \u003cdiv class=\"compose_discussion\" id=\"create_new_followup\"\u003eCompose a new followup discussion\u003c/div\u003e\n    \u003cdiv class=\"clarifying_discussion new clearFix\" id=\"create_new_followup_div\"\u003e\n      \u003cdiv class=\"account_image_container\"\u003e\n        {{#toUserPic user.id}}{{/toUserPic}}\n      \u003c/div\u003e\n      \u003cdiv class=\"discussion_content\" id=\"create_new_followup_editor\"\u003e\u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n");</script>

<script type="text/javascript">
P.modules.loadModule("text_editor", {}, null, "\n\n\u003cdiv data-pats=\"post_editor\" class=\"wdm_wrapper clearFix\"\u003e\n        \u003cdiv class=\"wmd-panel\"\u003e\n            \u003c!--\u003cdiv id=\"wmd-button-bar{{options.postfix}}\"\u003e\u003c/div\u003e--\u003e\n\u003ctextarea class=\"editor_textarea\" id=\"rich_{{options.postfix}}\" {{#if options.height}}style=\"height:{{options.height}}px;\"{{/if}}\u003e\u003c/textarea\u003e\n        \u003c/div\u003e\n        \u003c!--\u003cdiv id=\"wmd-preview{{options.postfix}}\" class=\"wmd-panel wmd-preview\"\u003e\u003c/div\u003e--\u003e\n        \u003cdiv class=\"actions\"\u003e\n          {{#unless options.noSave}}\n          \u003ca data-pats=\"post_link\" class=\"action save main\" href=\"#\" onclick=\"return P.text_editor.save('{{options.postfix}}')\"\u003e{{#if options.saveText}}{{options.saveText}}{{else}}save{{/if}}\u003c/a\u003e\n          {{#if options.anon}}\n           \u0026nbsp;\u0026nbsp;as \u003cselect id=\"anon_{{options.postfix}}\" class=\"anonymity_selector\"\u003e\n              {{#each options.anon}}\n              \u003coption value=\"{{this.val}}\"\u003e{{this.title}}\u003c/option\u003e\n              {{/each}}\n            \u003c/select\u003e\n          {{/if}}\n          \u003ca class=\"action preview_link\" href=\"#\" onclick=\"return P.text_editor.previewPost('{{options.postfix}}')\" style=\"margin-left:10px;\"\u003epreview\u003c/a\u003e\n          \u003ca class=\"action right cancel\" href=\"#\" onclick=\"return P.text_editor.cancel('{{options.postfix}}')\" style=\"margin-left:10px;\"\u003ecancel\u003c/a\u003e\n          {{#if isRich}}\n          \u003ca class=\"action right richtext_toggle\" href=\"#\" onclick=\"return PEM.fire('toggleRichtext')\"\u003euse plain text editor\u003c/a\u003e\n          {{else}}\n          \u003ca class=\"action right richtext_toggle\" href=\"#\" onclick=\"return PEM.fire('toggleRichtext')\"\u003euse rich text editor\u003c/a\u003e\n          {{/if}}\n          {{/unless}}\n        \u003c/div\u003e\n        \n      \u003c!-- Adding this so it shows up in every text editor thing and it doesn't have to be place everywhere individually--\u003e\n        \u003cdiv class=\"new_feature_notification_wrapper\" {{#unless showTip}}style=\"display:none;\"{{/unless}}\u003e\n          \u003cdiv class=\"new_feature_notification\"\u003e\n            \u003cspan class=\"new_feature_close\" onclick=\"return P.text_editor.closeHelpTip()\"\u003e×\u003c/span\u003e\n            \u003ch3 class=\"new_feature_header\"\u003eNew Feature: Tag Instructors\u003c/h3\u003e\n            \u003cdiv class=\"new_feature_description_container\"\u003e\n              \u003cp class=\"new_feature_description\"\u003e\n                \u003cspan class=\"inst_only_tag_helper\" style=\"display: none;\"\u003e\n                  Instructors can now tag fellow instructors in their posts by typing the @ symbol followed by their name, e.g. @Jane Smith.\n                \u003c/span\u003e\n                \u003cspan class=\"stud_only_tag_helper\"\u003e\n                  Students and instructors can now tag instructors in their posts by typing the @ symbol followed by their name, e.g. @Jane Smith.\n                \u003c/span\u003e\n                When an instructor is tagged, they receive a real-time email bypassing their email notifications.\n              \u003c/p\u003e\n              \u003cp class=\"new_feature_description\"\u003e\n                \u003cspan class=\"inst_only_tag_helper\" style=\"display: none;\"\u003e\n                  By default, only instructors can tag fellow instructors but you can enable students to also tag instructors if you wish.\n                  You can change default settings from the Manage Class tab.  \n                  Instructors will see instructor names and emails in the auto complete.\n                  If enabled for students, students will \u003cu\u003e\u003cb\u003eonly\u003c/b\u003e\u003c/u\u003e see instructor names and not instructor emails.\n                \u003c/span\u003e\n              \u003c/p\u003e\n              \u003cp class=\"new_feature_description\"\u003e\n                You can still link to posts by typing the @ symbol followed by the post number, e.g. @12\n              \u003c/p\u003e\n              \u003ca href=\"#\" class=\"new_feature_description\" style=\"margin-bottom: 10px; display: inline-block;\" onclick=\"return P.text_editor.closeHelpTip()\"\u003eOk got it\u003c/a\u003e\n              \u003cp class=\"new_feature_description\"\u003e\n                Feedback? Email us at \u003ca href=\"mailto:team@piazza.com\"\u003eteam@piazza.com\u003c/a\u003e\n              \u003c/p\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n\u003c/div\u003e\n");</script>

